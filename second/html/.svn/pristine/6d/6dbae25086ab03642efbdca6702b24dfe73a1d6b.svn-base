		/*弹框*/
		$('.header').on('click', '.other', function(event) {
			if( $(".header-pop-ups").css('display') == 'none' ){
				$(".header-pop-ups").slideDown();
			}
			else{
				$(".header-pop-ups").css('display','none');
			}
