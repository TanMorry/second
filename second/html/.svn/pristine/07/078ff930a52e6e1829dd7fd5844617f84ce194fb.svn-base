<script type="text/javascript">

	$(document).ready(function(){
		var mySwiper = new Swiper('.swiper-container', {
			speed: 333,
			autoplayDisableOnInteraction: false,
			resistanceRatio: 0,
			autoHeight: true,
			touchRatio : 0.35,
			onTransitionStart: function(swiper) {
				function change(one, two) {
					$('.think-to').removeClass(one).addClass(two);
				}

				if(swiper.activeIndex == 0) change('kind', 'love');
				else if(swiper.activeIndex == 1) change('love', 'kind');
			}
		});

		$('.think-to-first').on('click', 'a', function(event) {
			if($(this).hasClass('think-tf-love'))
			{
				mySwiper.slidePrev();
			}
			else if($(this).hasClass('think-tf-kind'))
			{
				mySwiper.slideNext();
			}
		});
	});
</script>
