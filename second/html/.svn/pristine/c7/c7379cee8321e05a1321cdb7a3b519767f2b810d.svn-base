/*登录菜单*/
.M_Login{
	position: relative;
	min-width: 960px;
}
.login-bg{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
}
.login-bg>img{
	width: 100%;
}
.login-bg .M_Center{
	position: relative;
}
.lm-back2{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
}
.lm-back2>div>b,
.lm-back2>div>a{
	display: block;
	position: absolute;
	top: 25px;
	left: 150px;
	width: 105px;
	height: 35px;
	z-index: 6;
}
.lm-back2>div>b{
	background: #eb6877;
	opacity: 0.65;
	filter:alpha(opacity=65);
}
.lm-back2>div>a{
	line-height: 35px;
	padding: 0 0 0 9px;
	color: #fff;
	background: url(../images/login.png) -42px -84px no-repeat;
}

.login-main{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	z-index: 4;
}
.login-menu{
	width: 310px;
	margin: 94px auto;
}
.lm-main{
	position: relative;
	background: #fff;
	/*避免margin叠加问题*/
	overflow: hidden;
	zoom: 1;
}
.lm-txt{
	line-height: 20px;
	margin: 30px 0 35px;
	font-size: 20px;
}
.lm-index{
	margin: 0 20px 20px;
}
.lm-index input{
	width: 248px;
	height: 22px;
	line-height: 22px;
	padding: 5px 10px;
	border: #bababa solid 1px;
}
.lm-index>div{
	position: relative;
	height: 34px;
}
.lm-index>div input{
	position: absolute;
	top: 0;
	left: 0;
}
.lm-hint{
	color: #adadad;
	z-index: 4;
	filter:alpha(opacity=100);
	opacity: 1;
}
.lm-input{
	color: #2d2d2d;
	z-index: 5;
	/*默认透明*/
	filter:alpha(opacity=0);
	opacity: 0;
}
.lm-input.lm-error{
	border-color: #eb6877;
}
.lm-be .lm-input{
	filter:alpha(opacity=100) !important;
	opacity: 1 !important;
}
.lm-be .lm-hint{
	filter:alpha(opacity=0) !important;
	opacity: 0 !important;
}
.lm-forget,
.lm-reg{
	font-size: 12px;
	color: #c0c0c0;
}
.lm-reg-shop{
	font-size: 12px;
	color: #eb6877;
}
.lm-forget{
	float: left;
}
.lm-reg-shop,
.lm-reg{
	float: right;
}
.lm-submit{
	display: block;
	width: 145px;
	height: 35px;
	line-height: 35px;
	margin: 0 auto 35px;
	color: #fff;
	background: #eb6877;
}
.lm-third a{
	display: inline-block;
	width: 25px;
	height: 25px;
	margin: 0 10px;
}
.lm-third {
	position: relative;
	margin: 0 0 20px;
}
.lm-third span{
	position: absolute;
	top: 0;
	left: 20px;
	height: 25px;
	line-height: 25px;
	font-size: 12px;
	color: #b5b5b5;
}
.lm-sina{
	background: url(../images/login.png) 0 -58px no-repeat;
}
.lm-qq{
	background: url(../images/login.png) -26px -58px no-repeat;
}
.lm-wx{
	background: url(../images/login.png) -52px -58px no-repeat;
}
.lm-to-shop{
	position: relative;
	width: 100%;
	height: 35px;
}
.lm-to-shop i{
	position: absolute;
	top: 0;
	right: 15px;
	width: 10px;
	height: 35px;
	background: url(../images/login.png) 0 -85px no-repeat;
}
.lm-to-shop b,
.lm-to-shop a{
	position: absolute;
	top: 0;
	left: 0;
	display: block;
	width: 100%;
	height: 35px;
}
.lm-to-shop b{
	background: #eb6877;
	filter:alpha(opacity=65);
	opacity: 0.65;
}
.lm-to-shop a{
	line-height: 35px;
	font-size: 14px;
	color: #fff;
}
.lm-back{
	display: block;
	position: absolute;
	top: 10px;
	left: 15px;
	width: 12px;
	height: 19px;
	background: url(../images/login.png) -25px -93px no-repeat;
}

.login-qr{
	position: absolute;
	bottom: 15px;
	right: 0px;
	z-index: 6;
}
.lq-logo{
	height: 55px;
	background: url(../images/login.png) 0 0 no-repeat;
}
.lq-code{
	float: left;
	width: 73px;
	height: 73px;
	margin: 0 13px 0 0;
}
.lq-txt{
	float: left;
}
.lq-txt li{
	text-align: left;
	height: 14px;
	line-height: 14px;
}
.lq-txt li a{
	display: inline-block;
	line-height: 14px;
	font-size: 14px;
	background: #000;
}
.lq-txt1,
.lq-txt2{
	margin: 0 0 15px;
}
.lq-txt1 a{
	color: #fff;
}
.lq-txt2 a,
.lq-txt3 a{
	color: #ffecc0;
}

.add-cart{
