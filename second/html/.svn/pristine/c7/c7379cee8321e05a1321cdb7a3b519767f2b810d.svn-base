@charset "utf-8";
body{
	margin:0 auto;
}

.M_GoTop{
	display: block;
	width: 82px;
	height: 203px;
	background: url(../images/back-top.png) center center no-repeat;
}
/*全屏遮罩*/
.Full-Shade{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: #000;
	filter:alpha(opacity:50);
	opacity:0.5;
	z-index:998;
}

/*页头*/
.M_Header{
	position: relative;
	min-width: 960px;
	height: 105px;
}
.hd-bg{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: #ffecc0;
}
.hd-pink{
	height: 40px;
	background: #eb6877;
}
.hd-index{
	position: relative;
}
.hd-main{
	position: relative;
	/*浮动居中必备*/
	overflow: hidden;
}
.hd-m-logo,
.hd-m-btn{
	width: 130px;
	height: 65px;
	padding: 40px 0 0;
}
.hd-m-btn a{
	display: block;
	height: 65px;
	line-height: 65px;
	font-size: 20px;
	color: #8c642d;
}
.hd-m-btn:hover,
.hd-m-btn.fixed{
	-webkit-border-radius: 0 0 10px 10px;
	   -moz-border-radius: 0 0 10px 10px;
	    -ms-border-radius: 0 0 10px 10px;
	     -o-border-radius: 0 0 10px 10px;
	        border-radius: 0 0 10px 10px;
    -webkit-box-shadow: 0 2px 5px #ba4a57;
	   -moz-box-shadow: 0 2px 5px #ba4a57;
	    -ms-box-shadow: 0 2px 5px #ba4a57;
	     -o-box-shadow: 0 2px 5px #ba4a57;
	        box-shadow: 0 2px 5px #ba4a57;
	filter:progid:DXImageTransform.Microsoft.Shadow(Strength=3,Direction=90,Color='#ba4a57')
			progid:DXImageTransform.Microsoft.Shadow(Strength=3,Direction=180,Color='#ba4a57')
			progid:DXImageTransform.Microsoft.Shadow(Strength=3,Direction=270,Color='#ba4a57');
	background: #eb6877;
}
.hd-m-btn:hover a,
.hd-m-btn.fixed a{
	color: #fff;
}
.hd-m-logo{
	background: url(../images/miao.png) center center no-repeat !important;
}
.hd-nav-l{
	position: absolute;
	top: 0;
	left: 0;
	height: 40px;
}
.hd-nav-r{
	position: absolute;
	top: 0;
	right: 0;
	height: 40px;
}

.hd-nav-info>li,
.hd-nav-exit>li,
.hd-nav-not>li{
	float: left;
	height: 40px;
	line-height: 40px;
}
.hd-nav-info>li>a,
.hd-nav-exit>li>a,
.hd-nav-not>li>a{
	line-height: 40px;
	font-size: 14px;
	color: #fff;
}
.hd-nav-not>li>a{
	padding: 0 10px;
	border-left: #cb4b5a solid 1px;
}
.hd-nav-exit>li>a{
	margin: 0 10px;
}
.hd-nav-info>li>a{
	position: relative;
	margin: 0 10px;
}

.hd-nav-not>li:first-child a{
	border-right: #f7828f solid 1px;
	border-left: transparent solid 0px;
}
.hd-nav-exit>li img{
	vertical-align: middle;
	width: 25px;
	height: 25px;
	-webkit-border-radius: 12px;
	   -moz-border-radius: 12px;
	    -ms-border-radius: 12px;
	     -o-border-radius: 12px;
	        border-radius: 12px;
}
.hd-nav-msg b{
	position: absolute;
	top: -4px;
	right: -8px;
	height: 12px;
	line-height: 12px;
	padding: 1px;
	-webkit-border-radius: 4px;
	   -moz-border-radius: 4px;
	    -ms-border-radius: 4px;
	     -o-border-radius: 4px;
	        border-radius: 4px;
	font-size: 12px;
	color: #fff;
	background: #ac2635;
}
.hd-nav-cut a i{
	display: inline-block;
	width: 14px;
	height: 14px;
	margin-right: 8px;
	background: url(../images/shopping.png) 0 0 no-repeat;
}
.hd-cart{
	display: none;
	position: absolute;
	top: 100%;
	right: 0;
	width: 175px;
	padding: 10px;
	-webkit-border-radius: 8px 0 8px 8px;
	   -moz-border-radius: 8px 0 8px 8px;
	    -ms-border-radius: 8px 0 8px 8px;
	     -o-border-radius: 8px 0 8px 8px;
	        border-radius: 8px 0 8px 8px;
	background: #fff;
	z-index: 999;
}
.hd-cart ul{
	margin-bottom: -10px;
}
.hd-cart ul li{
	margin-bottom: 10px;
}
.hd-nav-cut.fixed{
	background: #fff;
}
.hd-nav-cut.fixed>a {
	color: #eb6877;
}
.hd-nav-cut.fixed>a i{
	background: url(../images/shopping.png) -16px 0 no-repeat;
}
.hd-nav-cut.fixed .hd-cart{
	display: block;
}
.hd-item-l{
	float: left;
	width: 135px;
}
.hd-item-l p{
	text-align: left;
	overflow: hidden;
	height: 16px;
	line-height: 16px;
	font-size: 12px;
}
.hd-item-name a{
	color: #818181;
}
.hd-item-money{
	color: #ff5b5b;
}
.hd-item-r{
	float: right;
	width: 32px;
	height: 32px;
	margin-left: 8px;
}
.hd-item-r img{
	width: 100%;
	height: 100%;
}

.M_Footer{
	width: 100%;
	min-width: 960px;
	background: #1b1b1b;
}
.ft-dl{
	float: left;
	text-align: left;
	width: 25%;
	padding: 45px 0 0;
}
.ft-dl dt{
	
	margin-bottom: 45px;
	color: #fff;
}
.ft-dl dd{
	float: left;
}
.ft-dd-i{
	width: 73px;
	height: 73px;
	margin-right: 35px;
}
.ft-dd-i img{
	width: 100%;
	height: 100%;
}
.ft-dd-o a,
.ft-dd-t a{
	display: block;
	line-height: 13px;
	margin-bottom: 16px;
	font-size: 13px;
	color: #ffecc0;
}
.ft-dd-t{
	margin-left: 65px;
}
.ft-dd-c *{
	display: block;
	margin-bottom: 12px;
	color: #fff;
}
.ft-dd-c a{
	line-height: 13px;
	font-size: 13px;
}
.ft-dd-c b{
	line-height: 16px;
	font-size: 16px;
	font-weight: bold;
}
.ft-dd-c i{
	line-height: 28px;
	font-size: 14px;
}
.ft-dd-l i{
	display: block;
	line-height: 13px;
	margin-bottom: 12px;
	font-size: 13px;
	color: #ffecc0;
}
.ft-dd-l p{
	text-align: center;
	margin-right: -15px;
}
.ft-dd-l p a{
	display: inline-block;
	width: 32px;
	height: 32px;
	margin-right: 15px;
}
.ft-share-s{
	background: url(../images/share.png) 0 0 no-repeat;
}
.ft-share-w{
	background: url(../images/share.png) -34px 0 no-repeat;
}
.ft-share-q{
	background: url(../images/share.png) -68px 0 no-repeat;
}

.M_Page{
	text-align: right;
	margin: 35px 0;
	padding: 17px;
	font-size: 12px;
}
.M_Page span{
	margin: 0 10px;
}
.M_Page a{
	display: inline-block;
	text-align: center;
	width: 25px;
	height: 25px;
	line-height: 25px;
	-webkit-border-radius: 6px;
	   -moz-border-radius: 6px;
	    -ms-border-radius: 6px;
	     -o-border-radius: 6px;
	        border-radius: 6px;
	border: #8d8d8d solid 1px;
	color: #8d8d8d;
}

.M_Page a.index{
	width: 50px;
}
.M_Page a.fixed,
.M_Page a:hover{
	border-color: #eb6877;
	background: #eb6877;
	color: #fff;
}

/*选项*/
.M_Column{
	padding: 10px;
	background: #fff;
}
.M_Column.border{
	border-bottom: #cdcdcd solid 1px;
}
.column-tab{
	padding: 0 20px;
	cursor: pointer;
}
.column-tab li{
	float: left;
	width: 110px;
	height: 30px;
	line-height: 30px;
	/*margin: 10px 0 10px 20px;*/
	margin: 0 0 0 20px;
}
.ct-icon1{
	background: url(../images/tab-icon.png) -133px 0px no-repeat;
}
.ct-icon2{
	background: url(../images/tab-icon.png) -133px -30px no-repeat;
}
.ct-icon3{
	background: url(../images/tab-icon.png) -133px -60px no-repeat;
}
.ct-icon1:hover,
.ct-icon1.fixed{
	background: url(../images/tab-icon.png) 0 0 no-repeat;
}
.ct-icon2:hover,
.ct-icon2.fixed{
	background: url(../images/tab-icon.png) 0 -30px no-repeat;
}
.ct-icon3:hover,
.ct-icon3.fixed{
	background: url(../images/tab-icon.png) 0 -60px no-repeat;
}

.column-between{
	float: left;
	margin: 0 15px 0 0;
}
.cb-ul li{
	float: left;
}
.cb-ul li input{
	width: 30px;
	height: 24px;
	line-height: 24px;
	padding: 0 0 0 20px;
	border: #cecece solid 3px;
	background: url(../images/CNY.png) 3px center no-repeat;
}
.cb-line{
	width: 8px;
	margin: 14px 0;
	border:#cecece solid 1px;
}

.column-search,
.column-screen{
	float: right;
}
.column-screen{
	margin: 0 45px 0 0;
}
.column-screen li{
	float: left;
	width: 110px;
	height: 30px;
	line-height: 30px;
	margin: 0 0 0 20px;
	font-size: 14px;
	background: #0094e6;
	color: #fff;
}
.column-screen .checked{
	background: #5cc5ff;
}
.column-tab .checked{
	background: url(../images/button/column-btn-bg.png) center center no-repeat;
}
.column-search{
	margin: 0 20px 0 0;
}
.column-search>*{
	float: left;
}
.cs-in{
	width: 168px;
	height: 30px;
	line-height: 30px;
	margin: 0 10px 0 0;
	padding: 0 15px;
	background: #e7e7e7 url(../images/search.png) 168px center no-repeat;
	-webkit-border-radius: 24px;
	   -moz-border-radius: 24px;
	    -ms-border-radius: 24px;
	     -o-border-radius: 24px;
	        border-radius: 24px;
}
.cs-btn{
	line-height: 30px;
	color: #000;
}

.bar-top{
	margin: 0 0 25px;
}
.bar-top ul{
	width: 980px;
	overflow: hidden;
}
.bar-top ul li{
	position: relative;
	float: left;
	width: 225px;
	height: 135px;
	margin-right: 20px;
	-webkit-border-radius: 10px;
	   -moz-border-radius: 10px;
	    -ms-border-radius: 10px;
	     -o-border-radius: 10px;
	        border-radius: 10px;
	overflow: hidden;
}
.bar-top ul li>*{
	position: absolute;
	display: block;
	top: 0;
	left: 0;
	width: 225px;
	height: 135px;
}

.bar-top ul li a{
	/*line-height双倍行高*/
	top: 40%;
	line-height: 135px;
	color: #fff;
	font-size: 18px;
	font-weight: bold;
}
.bar-top ul li b{
	top: 80%;
	background: #000;
	filter:alpha(opacity:65);
	opacity:0.65;
}

.M_Layout_hl{
	border-bottom: #cdcdcd solid 1px;
	text-align: left;
}
.M_Layout_hl span{
	float: left;
	height: 45px;
	line-height: 45px;
}
.M_Layout_hl span[type=text]{
	padding: 0 10px 0;
}
.M_Layout_hl span[type=icon1]{
	width: 20px;
	background: url(../images/layout-icon.png) 0 0 no-repeat;
}
.M_Layout_hl span[type=icon2]{
	width: 20px;
	background: url(../images/layout-icon.png) 0 -45px no-repeat;
}
.M_Layout_hl span[type=icon3]{
	width: 26px;
	background: url(../images/layout-icon.png) 0 -90px no-repeat;
}
.M_Layout_hl a{
	float: right;
	height: 45px;
	line-height: 45px;
	margin: 0 25px 0 0;
	font-size: 12px;
	color: #9a9a9a;
}

.M_Left{
	float: left;
	width: 142px;
	padding: 4px 4px 0;
}
.M_Right{
	float: right;
	width: 795px;
}

.mass-group{
	background: #fff;
}
.mg-head{
	height: 30px;
	line-height: 30px;
	font-size: 14px;
	color: #fff;
	background: #eb6877;
}
.mg-main{
	margin: 0 0 60px;
}
.mg-fold>i,
.mg-fold>b{
	float: left;
}
.mg-fold>i{
	width: 26px;
	height: 24px;
	border-right: #e5e5e5 solid 1px;
	border-bottom: #e5e5e5 solid 1px;
}
.mg-fold.mg-push i{
	background: url(../images/fold.png) 6px 0 no-repeat;
}
.mg-fold.mg-minus i{
	background: url(../images/fold.png) 6px -26px no-repeat;
}
.mg-push+ul{
	display: none;
}
.mg-minus+ul{
	display: block;
}
.mg-fold>b{
	text-align: left;
	width: 105px;
	height: 24px;
	line-height: 24px;
	padding: 0 0 0 10px;
	border-bottom: #e5e5e5 solid 1px;
	font-size: 13px;
	color: #4b4b4b;
}
.mg-ul{
	border-bottom: #e5e5e5 solid 1px;
	overflow: hidden;
}
.mg-ul li{
	text-align: left;
	margin: 4px 0 4px 63px;
}
.mg-ul li a{
	font-size: 12px;
	color: #ff8795;
}
.mg-search{
	position: relative;
	height: 30px;
	margin: 10px 0;
}
.mg-search input[type=text]{
	position: absolute;
	top: 0;
	left: 0;
	width: 92px;
	height: 30px;
	line-height: 30px;
	padding: 0 35px 0 15px;
	-webkit-border-radius: 24px;
	   -moz-border-radius: 24px;
	    -ms-border-radius: 24px;
	     -o-border-radius: 24px;
	        border-radius: 24px;
	font-size: 13px;
	color: #8e8e8e;
	background: #e7e7e7;
}
.mg-search a{
	position: absolute;
	top: 0;
	left: 112px;
	width: 21px;
	height: 30px;
	background: url(../images/search.png) 0 center no-repeat;
}
.mg-shop{
	padding: 20px 12px 0;
	background: #ebebeb;
	border-radius: 24px 24px 0 0;
}
.mg-shop h3{
	text-align: left;
	line-height: 24px;
	font-weight: bold;
}
.mg-shop p{
	text-align: left;
	line-height: 18px;
	margin: 0 0 15px;
	font-size: 12px;
	color: #2d2d2d;
}
.mg-shop a{
	position: relative;
	display: block;
	width: 90px;
	height: 29px;
	margin: 0 auto;
	background: url(../images/miao-call-me.png) center center no-repeat;
}
.mg-shop a b{
	position: absolute;
	bottom: 1px;
	left: 28px;
	height: 20px;
	line-height: 20px;
	font-size: 14px;
	color: #fff;
}

.M_ItemThree{
	width: 558px;
}
.M_ItemFour{
	width: 744px;
	margin: 0 auto;
}
.M_ItemFive{
	width: 930px;
	margin: 0 auto;
	padding: 10px 0 20px;
}
.M_ItemNull{
	height: 288px;
	padding: 100px 0;
	background: url(../images/item-null.png) 50% 50% no-repeat;
}

.M_Item{
	display: block;
	float: left;
	text-align: left;
	width: 166px;
	height: 247px;
	margin: 22px 8px 8px;
	border: transparent solid 2px;
}
.M_ItemSmall{
	display: block;
	float: left;
	text-align: left;
	width: 166px;
	height: 211px;
	margin: 22px 8px 8px;
	border: transparent solid 2px;
}
.M_ItemSmall:hover,
.M_Item:hover{
	border: #ff7c8b solid 2px;
}
.M_Item_1{
	width: 166px;
	height: 166px;
}
.M_Item_1 img{
	width: 100%;
	height: 100%;
}
.M_Item_2{
	line-height: 24px;
	padding: 0 0 0 6px;
	font-size: 16px;
	color: #ff5b5b;
}
.M_Item_3{
	line-height: 21px;
	padding: 0 0 0 6px;
	font-size: 14px;
	color: #504f4f;
}
.M_Item_4{
	line-height: 18px;
	padding: 0 0 0 6px;
	font-size: 12px;
	color: #a8a8a8;
}
.M_ItemTxt{
	margin: 25px 0 15px;
	font-size: 20px;
	text-align: left;
}

.M_Book{
	margin: 0 15px 18px;
}
.M_Book_1{
	width: 94px;
	height: 122px;
	margin: 0 15px 5px;
	margin: 0 0 5px;
	padding: 16px 13px;
	background: #f7f7f7;
	border: transparent solid 3px;
	text-align: center;
}
.M_Book_2{
	margin: 0 0 5px;
}
.M_Book_2 i{
	display: inline-block;
	padding: 2px 8px;
	-webkit-border-radius: 12px;
	   -moz-border-radius: 12px;
	    -ms-border-radius: 12px;
	     -o-border-radius: 12px;
	        border-radius: 12px;
	font-size: 14px;
	color: #fefefe;
	background: #8c949a;
}
.M_Book_3{
	margin: 0 0 4px;
	font-size: 12px;
	color: #8d8c8c;
}
.M_Book_4{
	font-size: 13px;
	color: #ff5b5b;
}
.M_Book:hover .M_Book_1{
	border-color: #eb6877;
}
.M_Book:hover .M_Book_2 i{
	background: #eb6877;
}

.M_Relate{
	float: left;
}
.M_Relate ul{
	width: 146px;
	margin: 15px 23px 28px;
	text-align: left;
}
.M_Relate li img{
	display: block;
	margin: 0 0 16px;
	border: transparent solid 3px;
}
.M_Relate li img:hover{
	border: #eb6877 solid 3px;
}
.M_Relate li a{
	line-height: 16px;
	font-size: 14px;
	color: #333;
}
.M_Relate li i{
	float: right;
	padding: 0 0 0 18px;
	font-size: 16px;
	line-height: 17px;
	color: #ff5b5b;
	background: url(../images/miao-relate-money.png) left center no-repeat;
}
/*店铺名样式*/
.M_Relate-store{
	width: 116px;
	margin: 146px 34px 0 42px;
}
.M_Relate-store a{
	display: block;
	font-size: 13px;
	color: #333;
}
.M_Relate-store a:first-child{
	width: 116px;
	height: 115px;
	margin: 0 0 10px;
}
.M_Relate-store a img{
	width: 100%;
	height: 100%;
	-webkit-border-radius: 10px;
	   -moz-border-radius: 10px;
	    -ms-border-radius: 10px;
	     -o-border-radius: 10px;
	        border-radius: 10px;
}
.M_Mass {
	padding: 25px 42px 41px;
}
.M_Mass li{
	float: left;
	margin: 0 15px 42px;
	font-size: 13px;
	color: #3b3b3b;
}
.M_Mass li img{
	display: block;
	width: 116px;
	height: 115px;
	margin: 0 0 10px;
	-webkit-border-radius: 10px;
	   -moz-border-radius: 10px;
	    -ms-border-radius: 10px;
	     -o-border-radius: 10px;
	        border-radius: 10px;
}

/*厂商 社团铺 头部*/
.shelf-info{
	position: relative;
	min-width: 960px;
	height: 170px;
}
.shelf-info>img{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 170px;
	z-index: 2;
}
.si-mass{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	padding: 20px 0 0;
	z-index: 3;
}
.si-shop{
	position: relative;
}
.si-shop>div{
	position: absolute;
	top: 0;
	left: 0;
	padding: 20px 0 0;
	z-index: 3;
}
/*厂商 社团铺 通用样式*/
.si-name{
	margin: 0 0 10px;
	font-size: 30px;
	color: #fff;
}
.si-time{
	font-size: 14px;
	color: #fbfe00;
}
.si-disc{
	margin: 10px 224px 0;
	line-height: 24px;
	font-size: 14px;
	color: #fff;
}
.si-img{
	position: absolute;
	top: 28px;
	left: 30px;
	width: 116px;
	height: 115px;
	-webkit-border-radius: 10px;
	   -moz-border-radius: 10px;
	    -ms-border-radius: 10px;
	     -o-border-radius: 10px;
	        border-radius: 10px;
    border: #fff solid 1px;
    overflow: hidden;
}
.si-right{
	position: absolute;
	top: 42px;
	right: 15px;
	color: #fff;
}
.si-call{
	position: relative;
	display: block;
	width: 90px;
	height: 29px;
	margin: 0 14px 10px;
	background: url(../images/miao-call-me.png) center center no-repeat;
}
.si-call b{
	position: absolute;
	bottom: 1px;
	left: 28px;
	height: 20px;
	line-height: 20px;
	font-size: 14px;
	color: #fff;
}

.si-follow{
