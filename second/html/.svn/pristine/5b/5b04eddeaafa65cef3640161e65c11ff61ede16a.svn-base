.btn-fenxiang{position: absolute;bottom: 0;left: 0;z-index: 20;width: 100%;display: none}

.header-top{background: #000000;}

.con-bg{background: rgba(51, 51, 51, 0.75);padding-bottom:1rem}

.header-top p{font-size: 0.9rem;color: #FFFFFF;padding:0.5rem 0;display: block;}

.more ul{width: 100%}

.more{;padding:2rem 1rem;display: -webkit-box; -webkit-box-pack:justify; }

.more li{width: 33.33%;font-size: 0.7rem;color:#ffffff;float: left;text-align: center;}

.more li a{display: block;height:4.5rem;width: 4.5rem;margin:0 auto;}

.more .btn1{background: url("../img/fengxiang.png") 0.5rem  0.5rem no-repeat;background-size: 14rem}

.more .btn2{background: url("../img/fengxiang.png") -5rem  0.5rem no-repeat;background-size: 14rem}

.more .btn3{background: url("../img/fengxiang.png") -10.2rem  0.5rem no-repeat;background-size: 14rem}

.more .btn4{background: url("../img/fengxiang.png") 0.5rem  -4.5rem no-repeat;background-size: 14rem}

.more .btn5{background: url("../img/fengxiang.png") -5rem  -4.5rem no-repeat;background-size: 14rem}

.more .btn6{background: url("../img/fengxiang.png") -10.25rem  -4.5rem no-repeat;background-size: 14rem}


.btn-bottom{background:#000000;;text-align: center;width: 70%;margin: 0 auto;border-radius: 0.3rem}
.btn-bottom>a{display: block;font-size: 0.9rem;color: #FFFFFF;padding:0.5rem 0;}


