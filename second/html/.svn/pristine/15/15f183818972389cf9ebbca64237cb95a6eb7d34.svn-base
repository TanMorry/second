		var url = "/tongrenmiao/index.php?g=Mall&m=index&a=goodsDetails";
		$.ajax({
			url:url,
			data:{'id':id},
			dataType:'json',
			type:'get',
		}).done(function(ret)
		{
			if(ret)
			{
				handHtml(ret);
				setmeal = ret.setmeal;
			}
			else
			{
				location.href = 'error.html?status=1';
			}
		})
	}
	//重构html
