		$("#attentionNum").html(list.shopInfo.shop_collect);
		$("#allBaby").html(list.shopInfo.babyNum);
		$("#shop_logo").attr('src',list.shopInfo.logo);
		//进入店铺要带上当前商品id，用于店铺页面返回该页面
		var proId = param.id;
		$(".shop_addr").attr('href','shop-xiangqiye/shop-xiangqing.html?shopid='+list.shopInfo.userid+'&productId='+proId);
