	{
		var url = "/tongrenmiao/index.php?g=Mall&m=index&a=seeAgain";
		if(loadType == 1)
		{
			//第一次加载查询产品的具体信息
			var data = {'pid':param.id,'cossover':cossover,'cp':cp};
		}
		else
		{
			//加载相同属性的产品
			var data = {'cossover':cossover,'cp':cp,'page':page};
		}
		$.ajax({
			url:url,
			data:data,
			type:'post',
			dataType:'json',
		}).done(function(res)
		{
			if(loadType == 1)
			{
				if(res.detailInfo == "")
				{
					$(".pullUpLabel").html('加载失败');
				}
				else
				{
					var str = "";
//					str += '<div class="detail-access detail-access-borderbtm clearfix">\
//							<div class="detail-access-content clearfix">\
//						<p>同人圈类型：'+res.detailInfo.circlecate+'</p><p>偏好：'+res.detailInfo.pianhao+'</p><p>原作：'+res.detailInfo.cossover+'</p><p>作者：'+res.detail.author+'</p><p>角色名：'+res.detail.cpname+'</p><p>CP：'+res.detail.cp+'</p><p>类型：'+res.detail.cate+'</p><p>语言：'+res.detail.languge+'</p><p>页数：'+res.detail.pagenum+'P</p><p>尺寸：'+res.detail.size+'</p><p>发售日：'+res.detail.saletime+'</p><p>作者简介：</p><p class="font-small">'+res.detail.autoinfo+'</p></div></div>';
					str +='<div class="detail love part" id="detailInfo">\
							<div class="detail-access detail-access-borderbtm clearfix">\
						<div class="detail-access-content clearfix">\
						<p>同人圈类型：corssover</p>\
				<p>偏好：腐向</p>\
				<p>原作：学界战线</p>\
				<p>作者：千早</p>\
				<p>角色名：史蒂芬 克劳斯</p>\
				<p>CP：史蒂芬X克劳斯，史蒂芬X钢铁侠</p>\
				<p>类型：本子 同人本</p>\
				<p>语言：简体中文</p>\
				<p>页数：280P</p>\
				<p>尺寸：A8</p>\
				<p>发售日：2016.04.01</p>\
				<p>作者简介：</p>\
					<p class="font-small">关于“如果我死去”这个话题，花京院是有问过的。他清楚地记得承太郎的眉头皱了皱，这使得他不得不补</p>\
				</div>\
				</div>'

					if(res.detailInfo.picArr)
					{
						var str2 = "";
						$.each(res.detailInfo.picArr,function(k,v)
						{
							str2 += '<a href="#"><img src="'+v+'"></a>'
						})
					}
					str += str2;
					str += '</div>';
					$("#detailInfo").html(str);
					$('#tuijian').html(tuijian(res.tuijian));
					//先加载在跳转到下一屏
					if( $('#page1').css('display') == 'block' && $('#page2').css('display') == 'none' ){
						$('#page2').css('display','block');
						var pageHeight = Math.ceil($('#page2').offset().top - $('#page1').offset().top - $('#moveMask').height());

						wrapper.scrollTo(0,-pageHeight-40,500,false);
						wrapper.refresh();
					}
				}
			}
			else
			{
				if(res.tuijian)
				{
					$('#tuijian').append(tuijian(res.tuijian));
					page++;
				}
				else
				{
					$(".pullUpLabel").html('已无更多数据');
				}
				wrapper.refresh();
			}
		}).fail(function()
		{

		})
	}

	function tuijian(list)
	{
		var str = "";
		$.each(list,function(k,v){
			str += '<li>\
					<div class="sale-ones-front tie">\
					<p class="sale-of-img"><img src="'+v.pic+'"></p>\
					<p class="sale-of-name">'+v.name+'</p>\
					<p class="sale-of-price">￥'+v.price+'</p>\
					</div>\
					</li>';
		})
		return str;
	}
</script>