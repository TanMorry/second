<script type="text/javascript">

$(document).ready(function(){
	/*验证码冷却倒计时*/
	function timeKeeping (jq, time) {
		var t = time;
		var $jq = jq;

		function finish () {
			$jq.text('重新发送').removeClass('disable');
		}

		function run () {
			$jq.text((t--) + '秒');
			t >= 0 ? setTimeout(run, 1000) : finish();
		}

		run();
	}
	$(".cb-c-get").on('click', function(event) {
		if(!$(this).hasClass('disable'))
		{
			timeKeeping($(this).addClass('disable'), 60);
		}
	});

	/*确认体现初始化*/
	var confirm = $("#cash-confirm").MiaoDialog();
	var success = $("#cash-success").MiaoDialog();
	/*事件绑定*/
	$('.cbs-cash').on('click', 'a', function(event) {
		confirm.MiaoDialog('open');
	});
	$('.cash-btn').on('click', 'a', function(event) {
		/*如果点确定则提示提现... 实际应该服务器确认返回后弹出*/
		if ($(this).attr('prop') == 'submit')
		{
			success.MiaoDialog('open');
		}
		
		confirm.MiaoDialog('close');
	});
	$('.cb-ok').on('click', 'a', function(event) {
		success.MiaoDialog('close');
	});
});

</script>

