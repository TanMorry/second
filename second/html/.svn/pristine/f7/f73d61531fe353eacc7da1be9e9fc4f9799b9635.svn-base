<script>
	var username = "lalala";
	var userid = 2;
	$(function(){
		shopInfo();
	})
	//第一次请求所有店铺和该店铺的三个最新上架的商品和该商品是否被收藏
	function shopInfo()
	{
		var data={};
		data['uid'] = userid;
		data['type'] = 2;//商业店
//		var shopUrl = RequestUrl('shop');
		var shopUrl = '/tongrenmiao/index.php?g=Mall&m=Index&a=business'
		$.ajax({
			url:shopUrl,
			data:data,
			type:'get',
			dataType:'json',
			beforeSend:function()
			{
				$("#content").html("<img src='../img/loading.jpg'>");
				$("#content").css('background-color','#fff');
			},
			success:function(res)
			{
				if(res.Info != "")
				{
					handHtml(res.Info)
				}
				else
				{
					$("#content").html('<div class="load-fail">没有找到该店铺商品</div><div>');
				}
			},
			error:function()
			{

			}
		})
	}

	function handHtml(list)
	{
		var str = "";
		$.each(list,function(ke,va)
		{
			str += '<div class="store-search part clearfix">\
						<div class="store-box clearfix">\
							<div class="store-img">\
								<a href="../shangye_xiangqi.html?shopId='+va.userid+'"><img src="'+va.logo+'" alt=""></a>\
							</div>\
							<div class="store-info">\
								<p class="store-i-name">'+va.company+'</p>\
								<p class="store-i-text">关注人数 '+va.shop_collect+'</p>\
								<p class="store-i-text">全部宝贝 '+va.allbaby+'</p>\
							</div>'
			if(va.collectStatus == 1)
			{
				str += '<div class="store-up"><a class="btn-color" href="#" data-id="' + va.userid + '">收 藏</a></div>'
			}
			else
			{
				str += '<div class="store-up"><a href="#" data-id="' + va.userid + '">收 藏</a></div>'
			}
				str += '</div>\
						<div class="store-box clearfix">\
							<ul class="sale-ones clearfix">'
								$.each(va.product,function(k,v)
								{
									str += '<li><div class="sale-ones-front"><p class="sale-of-img"><a href="../detail-page1.html?id='+v.id+'"><img src="'+v.pic+'"></a></p></div></li>';
								})

						str +='	</ul></div>	</div>';
		})
		$('#content').html(str);
		
		$('.store-up').on('click', 'a', function(){
			var obj = $(this);
	        var data = {};
	        data['collect'] = $(this).hasClass('btn-color') ? 0 : 1;
	        data['userid'] = userid;
	        data['uname'] = username;
	        data['shopid'] = $(this).attr('data-id');
	        data['shopname'] = $(this).parents('.store-up').siblings('.store-info').find('.store-i-name').text();
	        $.ajax({
				url:"/tongrenmiao/index.php?g=Mall&m=index&a=isCollect",
				data:data,
				dataType:'json',
				type:'get',
				beforeSend:function()
				{
					
				},
				success:function(data)
				{
					if(data.collect == '0')
					{
						//取消收藏
						alertInfo('收藏已取消');
					}
					else
					{
						alertInfo('已收藏');
					}
					obj.toggleClass('btn-color');
				},
				error:function()
				{

				}
			})
	    });
	}

	//提示框
	function alertInfo(mes)
	{
		var phone2 = $('#cpp2').MiaoDialog();
		phone2.MiaoDialog('open');
		$('#cpp2').find('p').text(mes);
		setTimeout(function(){
			phone2.MiaoDialog('close');
		},1400);
	}
</script>