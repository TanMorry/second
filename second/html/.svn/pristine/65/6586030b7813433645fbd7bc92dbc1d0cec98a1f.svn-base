<script>
    var userid = 22;
    var urlInfo = new Arguments();
    var param = urlInfo.Request();
    var state;
    $(function()
    {
        IndexList(param.code);
    })

    refresher.init({
        id: "wrapper",
        header: true,
        footer: false,
        pullDownAction: Refresh,
    });

    //下拉刷新
    function Refresh() {
        setTimeout(function () {
            wrapper.refresh();
            IndexList(state);
        }, 500);
    }

    function IndexList(param)
    {
        $(".content").find('section[data-code='+param+']').show().siblings().hide();
        $(".header-nav").find('div[data-code='+param+']').addClass('content-nav-btn').siblings().removeClass('content-nav-btn');
        var url='/tongrenmiao/index.php?g=Mall&m=Index&a=order_list';
        var data = {};
        data['userid'] = userid;
        data['search_status'] = param;
        $.ajax({
            url: url,
            data: data,
            type: 'get',
            dataType: 'json',
            beforeSend: function ()
            {
                $("#loading").show();
            },
            success: function (res)
            {
                state = param;
                $("#loading").hide();
                if (res.Info)
                {
                    handleHtml(res.Info,param);
                    wrapper.refresh();
                }
                else
                {
                    //没有订单
                }
            },
            error: function () {

            }
        })
    }

    $(".header-nav >div").click(function(){
        IndexList($(this).attr('data-code'));
    })

    //重构html
    function handleHtml(list,element)
    {
        var str = "";
        $.each(list,function(k,v)
        {
            str += '<div class="content-list">\
                        <div class="commodity content-top">\
                            <div class="commodity-l">\
                                <p class="commodity-l-p">\
                                    <a href="shangye_xiangqi.html?shopId='+v.sid+'"><span>'+v.company+'</span></a>\
                                    <img src="img/you-iocn.png">\
                                </p>\
                            </div>\
                            <div class="commodity-t" >\
                                <span>'+v.statusInfo+'</span>\
                            </div>\
                        </div>'
            $.each(v.detail,function(ke,va)
            {
                str += '<div class="content-choice clearfix border">\
                            <div class="clearfix">\
                                <div class="content-choice-c">\
                                    <p><a href="detail-page1.html?id='+va.pid+'"><img src="'+va.pic+'"></a></p>\
                                </div>\
                                <div class="content-choice-w">\
                                    <p class="choice-w-one content-choice-p">'+va.pname+'</p>\
                                    <p class="choice-w-two content-choice-p">'+va.spec_value+'</p>\
                                    <p class="choice-w-three">\
                                        <span>￥'+va.price+'</span>\
                                    </p>\
                                </div>\
                                <div class="commodity-r commodity-r-money">\
                                    <i>X '+va.num+'</i>\
                                </div>\
                            </div>\
                        </div>'
            })
            str += diff_order_list(v);
            str += '</div>'
        })
//        $(".content").find('section[data-code='+param+']').html(str);
        $(".content").find('section[data-code='+element+']').html(str).show().siblings().hide();
    }

    //不同状态订单列表处理
    function diff_order_list(list)
    {
        var yunfei;
        if(list.logistics_price >0)
        {
            yunfei = '含运费￥'+list.logistics_price;
        }
        else
        {
            yunfei = '免运费';
        }
        str = '<div class="content-xixnxi content-shop content-xixnxi-border ">共<span>'+list.count+'</span>件商品 合计￥<span>'+list.product_price+'</span> （'+yunfei+'）</div>';
        //页面只显示四种  待付款  待发货  待收货  待评价

        if(list.status == 1)
        {
            //待付款
            str += '<div class="content-xixnxi clearfix  content-xixnxi-border2" data-orderId="'+list.order_id+'">\
                        <p>\
                            <a href="javascript:">联系卖家</a>\
                            <a href="javascript:"  class="cancel" onclick="order_cancle(this)">取消订单</a>\
                            <a href="javascript:" class="more-color">待付款</a>\
                        </p>\
                    </div>';
        }
        if(list.status == 2)
        {
            //待发货
            str += '<div class="content-xixnxi clearfix content-xixnxi-border2">\
                        <p>\
                            <a href="javascript:">提醒发货</a>\
                            <a href="javascript:">联系卖家</a>\
                        </p>\
                    </div>'

        }
        if(list.status == 3)
        {
            //待收货
            str += '<div class="content-xixnxi clearfix  content-xixnxi-border2">\
                        <p>\
                            <i class="content-sh-btn">\
                                <span class="content-sh-tc">\
                                    <a href="javascript:" class="content-tc-border content-bf">联系卖家</a>\
                                    <a href="javascript:">申请退货</a>\
                                </span>\
                            </i>\
                            <a href="javascript:" class="delay">延迟收货</a>\
                            <a href="javascript:">查看物流</a>\
                            <a href="javascript:" class="more-color">确认收获</a>\
                        </p>\
                    </div>'
        }
        if(list.status == 4)
        {
            //待评价
            str += '<div class="content-xixnxi clearfix  content-xixnxi-border2">\
                        <p>\
                            <a href="javascript:">申请退货</a>\
                            <a href="javascript:"  class="more-color">评价</a>\
                        </p>\
                    </div>'
        }
        return str;
    }

    //取消订单
    function order_cancle(obj)
    {
        var text = $(".cancel-reason");
        var none=$(' .cover-more');
        var btn =$("#cancle_finish");
        var reason = '取消';//暂定
        var orderId = $(obj).parent().parent().attr('data-orderId');
        var $this = $(obj).parents('div .content-list');
        none.show();
        text.slideDown();
        none.click(function () {
            text.slideUp();
            none.hide()
        })
        btn.click(function(){
            ajax_del_order(orderId,$this,reason);
            text.hide()
            none.hide()
        });
    }
    function ajax_del_order(orderId,$this,reason)
    {
        url = '/tongrenmiao/index.php?g=Mall&m=Index&a=cancel_order';
        $.ajax({
            url:url,
            data:{'order_id':orderId,'reason':reason},
            type:'get',
            dataType:'json',
            beforeSend:function()
            {
                //等待图标
            },
            success:function(res)
            {
                if(res>0)
                {
                    alertInfo(' 取消订单成功');
                    setTimeout(function(){
                        $this.remove();
                    },1500);
                }
            },
            error:function()
            {

            }
        })
    }

    //提示框
    function alertInfo(mes)
    {
        var phone2 = $('#cpp2').MiaoDialog();
        phone2.MiaoDialog('open');
        $('#cpp2').find('p').text(mes);
        setTimeout(function(){
            phone2.MiaoDialog('close');
        },1400);
    }
</script>


