    background: url(../images/file1.png) 0 0 no-repeat;
    cursor: pointer;
}
.wp-file input[type=file]{
    position:absolute;
    top: 0;
    left: 0;
    width: 0;
    height: 0;
    font-size: 0;
    opacity: 0;
    filter: alpha(opacity=0);
    z-index: 1;
}
.wp-file img{
    position:absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 2;
}

.wp-block{
