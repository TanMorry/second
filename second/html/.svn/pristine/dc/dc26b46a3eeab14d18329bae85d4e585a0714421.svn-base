@charset "utf-8";
body{
    margin:0 auto;
}

.M_Left{
    float: left;
    width: 150px;
    margin: 0;
    padding: 0;
}
.M_Right{
    float: right;
    width: 805px;
}

.fixed-table{
    table-layout: fixed;
    width: 100%;
}
.ol-tb-head th,
.cbs-tb th,
.se-tb th,
.gc-tb th,
.cb-tb th{
    font-size: 13px;
    line-height: 13px;
    padding: 14px 0;
    color: #fff;
}

.seller-nav{
    margin-bottom: 10px;
    border-bottom: #000 solid 2px;
}
.sn-l{
    float: left;
    width: 105px;
    height: 45px;
    line-height: 45px;
    font-size: 20px;
    color: #fff;
    background: #000;
}
.sn-r{
    float: right;
    height: 45px;
    line-height: 45px;
}
.sn-r a{
    color: #000;
}
.sn-r i{
    display: inline-block;
    overflow: hidden;
    vertical-align: text-bottom;
    width: 20px;
    height: 20px;
    line-height: 20px;
    margin-right: 10px;
    -webkit-border-radius: 12px;
    -moz-border-radius: 12px;
    -ms-border-radius: 12px;
    -o-border-radius: 12px;
    border-radius: 12px;
}
.sn-r i img{
    width: 100%;
    height: 100%;
}

.seller-panel{
    position: relative;
    overflow: hidden;
    height: 170px;
}
.sp-bg,
.sp-lm{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}
.sp-lm{
    opacity: 0.65;
    filter: alpha(opacity=65);
    background: #000;
}
.sp-index{
    position: absolute;
    top: 50%;
    left: 0;
    margin-top: -57px;
    color: #fff;
}
.sp-i-img{
    overflow: hidden;
    float: left;
    width: 116px;
    height: 115px;
    margin: 0 30px;
    -webkit-border-radius: 8px;
    -moz-border-radius: 8px;
    -ms-border-radius: 8px;
    -o-border-radius: 8px;
    border-radius: 8px;
    border: #fff solid 1px;
}
.sp-i-img img{
    width: 100%;
    height: 100%;
}
.sp-i-txt{
    float: left;
    text-align: left;
}
.sp-i-t1{
    line-height: 30px;
    margin-bottom: 30px;
    font-size: 30px;
    font-weight: bold;
}
.sp-i-t2{
    line-height: 14px;
    margin-bottom: 14px;
    font-size: 14px;
    word-spacing: 24px;
}
.sp-right{
    position: absolute;
    top: 0;
    right: 0;
    line-height: 14px;
    padding: 20px;
    color: #fff;
    font-size: 14px;
}

.left-tree{
    background: #fff5e7;
    padding-bottom: 50px;
}
.left-tree dl{
    text-align: left;
}
.left-tree dt a{
    display: block;
    height: 32px;
    line-height: 32px;
    padding-left: 32px;
    font-size: 14px;
    color: #000;
}
.left-tree dd a{
    display: block;
    height: 32px;
    line-height: 32px;
    padding-left: 52px;
    font-size: 12px;
    color: #818181;
}
.left-tree dt a.fixed,
.left-tree dd a.fixed,
.left-tree dt a:hover,
.left-tree dd a:hover{
    background: #fff;
}

.seller-left{
    float: left;
    overflow: hidden;
    padding: 0 20px;
    width: 525px;
}
.seller-right{
    overflow: hidden;
    height: 100%;
}
.sl-body{
    margin-bottom: -1px;
}
.sl-part{
    text-align: left;
    padding: 20px 0;
    border-bottom: #cdcdcd solid 1px;
}
.sl-p-cap{
    line-height: 16px;
    margin-bottom: 16px;
    font-size: 16px;
    color: #141414;
}
.sl-p-news{
    line-height: 24px;
    padding: 5px 10px;
    font-size: 12px;
    background: #f8f8f8;
}
.sl-p-news p{
    color: #545454;
}
.sl-p-news a{
    color: #eb6877;
}
.sl-p-news a i{
    color: #6d6d6d;
}
.sl-p-btn p{
    float: left;
    min-width: 33.33%;
    margin-top: 10px;
    height: 12px;
    line-height: 12px;
}
.sl-p-btn a{
    margin-left: 15px;
    font-size: 12px;
    color: #545454;
}

.sr-part{
    text-align: left;
    margin-bottom: 20px;
    border: #ff7c8b solid 2px;
}
.sr-part p,
.sr-part a,
.sr-part b{
    margin-bottom: 15px;
    padding-left: 15px;
}
.sr-part p{
    line-height: 40px;
    font-size: 14px;
    color: #fff;
    background: #ff7c8b;
}
.sr-part a,
.sr-part b{
    display: block;
    line-height: 18px;
    color: #141414;
    font-size: 12px;
}
/*换行位置*/
.sr-part b{
    padding-left: 75px;
}
.sr-part b i{
    margin-left: -60px;
    color: #8c642e;
}
.sr-part .part1{
    background: #ff7c8b url(../images/seller-part.png) 95% 0 no-repeat;
}
.sr-part .part2{
    background: #ff7c8b url(../images/seller-part.png) 95% -40px no-repeat;
}

.seller-site{
    text-align: left;
    padding: 20px;
}
.seller-site a{
    line-height: 16px;
    font-size: 16px;
}
.ss-base:after,
.ss-next:after{
    content: '\>';
    display: inline-block;
    margin-left: 5px;
}
a.ss-base{
    color: #000000;
    text-decoration: underline;
}
a.ss-next{
    color: #000000;
}
a.ss-last{
    color: #eb6877;
}

.sort-edit{
    padding: 10px;
}

.se-tb{
    background: #f8f8f8;
}
.se-tb th{
    background: #ff7c8b;
}
.se-tb td{
    padding: 8px;
}
.se-tb tr td:first-child{
    text-align: left;
}
/*table input通用*/
.se-tb input[type=text]{
    vertical-align: middle;
    padding: 5px 10px;
    border: #aeaeae solid 1px;
}
.se-name{
    width: 70%;
}
.se-rank{
    width: 15%;
}
.se-oper{
    width: 15%;
}

.se-n-left{
    display: inline-block;
    width: 45px;
    padding-left: 10px;
}
.se-n-s{
    display: inline-block;
    vertical-align: middle;
    position: relative;
    width: 18px;
    height: 18px;
}
.se-n-s .n-s1,
.se-n-s .n-s2,
.se-n-s .n-s3,
.se-n-s .n-s4{
    position: absolute;
    width: 8px;
    height: 8px;
}
.se-n-s .n-s1{
    top: 0;
    left: 0;
    background: #ffffff;
}
.se-n-s .n-s2{
    top: 0;
    right: 0;
    background: #ffd1d1;
}
.se-n-s .n-s3{
    bottom: 0;
    left: 0;
    background: #ff7c8b;
}
.se-n-s .n-s4{
    bottom: 0;
    right: 0;
    background: #d24d5d;
}
.se-n-t{
    font-size: 14px;
    color: #616161;
}
.se-n-i{
    vertical-align: bottom;
    font-size: 12px;
    color: #eb6877;
}

.se-txt{
    width: 278px;
    height: 23px;
    line-height: 23px;
}
.se-num{
    width: 53px;
    height: 23px;
    line-height: 23px;
}
.se-btn{
    width: 70px;
    height: 30px;
    margin-left: 10px;
    color: #fff;
    background: #eb6877;
    cursor: pointer;
}
.se-o-a{
    font-size: 12px;
    color: #eb6877;
}

.seller-grade{
    padding: 20px;
}
.sg-item{
    margin-bottom: 10px;
    padding-top: 25px;
    font-size: 12px;
}
.cb-cap,
.sg-cap{
    text-align: left;
    font-size: 16px;
}
.cb-cap{
    padding: 20px;
}
.sg-l{
    float: left;
    text-align: right;
}
.sg-r{
    position: relative;
    float: right;
    width: 485px;
}
.sg-l-p1{
    color: #616161;
}
.sg-l-p2{
    color: #eb6877;
}

.sg-r-for{
    position: absolute;
    top: -25px;
    left: 0%;
    width: 35px;
    height: 25px;
    line-height: 17px;
    /* -45px + 35px/2  */
    margin-left: -63px;
    color: #fff;
    background: url(../images/grade-bar.png) 0 -17px no-repeat;
}
.sg-r-bar{
    line-height: 16px;
    margin-bottom: 10px;
    background: url(../images/grade-bar.png) -45px 0 no-repeat;
}
.sg-r-var{
    line-height: 12px;
}
.sg-r-bar i,
.sg-r-var i{
    float: left;
    width: 20%;
}

.gc-tb th{
    width: 16.66%;
    background: #8c642d;
}
.gc-tb td{
    font-size: 12px;
    line-height: 12px;
    padding: 10px 0;
    color: #616161;
    border-bottom: #b7b7b7 solid 1px;
}

.tb-font1{color: #ff7c8b !important;}
.tb-font2{color: #8c642d !important;}
.tb-font3{color: #000000 !important;}
.tb-font4{color: #0d7ec6 !important;}

.cb-tb th{
    background: #ff7c8b;
}
.cb-tb td{
    font-size: 12px;
    line-height: 12px;
    padding: 10px 0 2px;
    color: #616161;
}
.cb-tb p{
    margin-bottom: 8px;
}
.cb-tb .gray{
    background: #f8f8f8;
}
.cb-h1{width: 5%;}
.cb-h2{width: 40%;}
.cb-h3{width: 15%;}
.cb-h4{width: 25%;}
.cb-h5{width: 15%;}

.cb-s1{width: 15%;}
.cb-s2{width: 45%;}
.cb-s3{width: 15%;}
.cb-s4{width: 15%;}

.cb-t-cmt{
    text-align: left;
}
.cb-t-name{
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: keep-all;
    white-space: nowrap;
}
.cb-t-price{
    color: #eb6877;
}

.cb-shou{
    padding: 35px 0;
}
.cbs-show{
    line-height: 16px;
    margin-bottom: 25px;
    font-size: 16px;
    color: #616161;
}
.cbs-show i{
    font-size: 20px;
    margin: 0 5px;
    color: #eb6877;
}
.cbs-cash a{
    line-height: 16px;
    padding: 7px 20px;
    color: #fff;
    background: #eb6877;
}

.cbs-i-p{
    padding: 0 20px;
    margin-bottom: 20px;
}
.cbs-p-l{
    float: left;
    font-size: 14px;
}
.cbs-p-r{
    float: right;
    line-height: 16px;
}
.cbs-p-c{
    line-height: 16px;
}
.cbs-p-c a,
.cbs-p-r a{
    float: left;
    line-height: 16px;
    padding: 0 10px;
    color: #828282;
}
.cbs-p-c a:first-child,
.cbs-p-r a:first-child{
    border-right: #a6a6a6 solid 1px;
}
.cbs-p-c a.fixed,
.cbs-p-c a:hover,
.cbs-p-r a.fixed,
.cbs-p-r a:hover{
    color: #eb6877;
    text-decoration: underline;
}
.cbs-i-more{
    float: right;
    margin: 20px 15px 10px;
    font-size: 12px;
    color: #ff7c8b;
}

.cb-cash{
    /*width: 400px;*/
    /*margin: 0 auto;*/
}
.cb-c-b{
    margin-bottom: 20px;
}
.cb-c-l{
    float: left;
    text-align: right;
    width: 40%;
    line-height: 35px;
    margin-right: 15px;
    font-size: 12px;
}
.cb-c-r{
    float: left;
}
.cb-c-in{
    display: block;
    width: 203px;
    height: 23px;
    line-height: 25px;
    padding: 5px 10px;
    border: #a9a9a9 solid 1px;
}
.cb-c-lab{
    display: block;
    width: 225px;
}
.cb-c-num{
    float: left;
    line-height: 35px;
    font-size: 14px;
    color: #545454;
}
.cb-c-get{
    float: right;
    width: 75px;
    height: 25px;
    line-height: 25px;
    margin: 5px 0;
    color: #fff;
    font-size: 12px;
    background: #ffa3ae;
}
.cb-c-get.disable{
    color: #bdbdbd;
    background: #eee;
}
.cb-c-msg{
    display: block;
    text-align: left;
    line-height: 24px;
    font-size: 12px;
    color: #ff7c8b;
}

.cb-modal{
    display: none;
    width: 375px;
    height: 365px;
    -webkit-border-radius: 24px;
    -moz-border-radius: 24px;
    -ms-border-radius: 24px;
    -o-border-radius: 24px;
    border-radius: 24px;
    background: #fff;
}

.cash-cap{
    line-height: 20px;
    font-size: 20px;
    padding: 70px 0 35px;
    color: #141414;
}
.cash-p{
    text-align: left;
    line-height: 36px;
    padding-left: 50%;
    font-size: 12px;
    color: #545454;
}
.cash-p i{
    display: inline-block;
    text-align: right;
    width: 50%;
    margin: 0 15px 0 -50%;
}
.cash-btn{
    padding-top: 35px;
}
.cash-btn a{
    display: inline-block;
    width: 90px;
    height: 35px;
    line-height: 35px;
    margin: 0 15px;
    color: #fff;
    background: #eb6877;
}

.cb-success{
    overflow: hidden;
    width: 100%;
    height: 100%;
    background: url(../images/applyshop.png) 50% 50% no-repeat;
}
.cb-s-p1{
    margin: 40px 0 15px;
    font-size: 20px;
    color: #141414;
    background: url(../images/true.png) 78% 50% no-repeat;
}
.cb-s-p2{
    margin: 0 0 200px;
    font-size: 14px;
    color: #878787;
}
.cb-ok{
    height: 35px;
}
.cb-ok a{
    display: inline-block;
    min-width: 70px;
    height: 35px;
    line-height: 35px;
    padding: 0 10px;
    color: #fff;
    background: #eb6877;
}

.cb-add-phone{
    overflow: hidden;
    width: 100%;
    height: 100%;
    background: url(../images/bg-miao1.png) 50% 60% no-repeat;
}
.cb-ap-p1,
.cb-ap-p2{
    font-size: 20px;
    color: #141414;
}
.cb-ap-p1{
    margin: 60px 0 25px;
}
.cb-ap-p2{
    margin-bottom: 150px;
}
.cb-ap-p2 a{
    font-size: 20px;
    border-bottom: #eb6877 dashed 1px;
    color: #eb6877;
}

.release-step{
    overflow: hidden;
    padding: 25px 0;
    background: #fff;
}
.release-step ul{
    width: 125%;
}
.release-step li{
    float: left;
    width: 320px;
    height: 40px;
    line-height: 40px;
    margin-right: 2px;
    color: #fff;
    background: #a7a7a7;
}
.release-step li i{
    display: inline-block;
    vertical-align: text-bottom;
    width: 22px;
    height: 22px;
    line-height: 22px;
    margin-right: 8px;
}
.release-step li.fixed{
    background: #ff7c8b;
}
.release-step li i.one{
    background: url(../images/punctuation.png) -24px 50% no-repeat;
}
.release-step li i.two{
    background: url(../images/punctuation.png) -48px 50% no-repeat;
}
.release-step li i.three{
    background: url(../images/punctuation.png) -72px 50% no-repeat;
}

.release-sort{
    margin: 0 25px;
    padding-bottom: 45px;
}
.rs-bg{
    background: #f8f8f8;
}
.rs-fast{
    overflow: hidden;
    padding: 5px 0;
}
.rs-f-i{
    line-height: 35px;
    margin-right: 8px;
}
.rs-f-s{
    width: 588px;
    height: 35px;
    line-height: 35px;
    border: #aeaeae solid 1px;
    color: #898989;
}
.rs-all{
    overflow: hidden;
}
.rs-a-ul{
    overflow: auto;
    text-align: left;
    width: 220px;
    height: 250px;
    margin: 0 18px;
    padding: 10px 0;
    -webkit-border-radius: 10px;
    -moz-border-radius: 10px;
    -ms-border-radius: 10px;
    -o-border-radius: 10px;
    border-radius: 10px;
    border: #aeaeae solid 1px;
    background: #fff;
}
.rs-a-ul li{
    overflow: hidden;
    height: 30px;
    line-height: 30px;
}
.rs-a-ul li a.fixed{
    color: #fff;
    background: #ff7c8b;
}
.rs-a-ul li a{
    display: block;
    padding: 0 10px;
    font-size: 14px;
    color: #353535;
}
.rs-a-ul li a:before{
    content: '\>';
    float: left;
    margin-right: 10px;
}

.rs-say{
    text-align: left;
    padding: 10px 85px 0;
}
.rs-s-icon{
    border-bottom: #aeaeae solid 1px;
}
.rs-s-icon i{
    display: block;
    width: 20px;
    height: 18px;
    margin: 0 0 -2px 5%;
    border-bottom: #fff solid 1px;
    background: url(../images/say1.png) 0 0 no-repeat;
}
.rs-s-now{
    padding: 10px 20px;
    border-color: #aeaeae;
    border-style: solid;
    border-width: 0px 1px 1px 1px;
    font-size: 14px;
    color: #eb6877;
    background: #fff;
}

.write-extra{
    padding: 20px 20px 0;
}
