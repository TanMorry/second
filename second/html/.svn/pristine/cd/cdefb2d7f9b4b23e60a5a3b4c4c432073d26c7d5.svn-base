        $().ready(function () {
            var h1 = $(".header-btn-a>a");
            var text = $(".header-in-btn");
            h1.click(function () {
                text.slideDown();
                text.focus();
            });
            text.on('click', 'a', function () {
                var t = $(this).text();
                h1.text(t);
                text.hide();
            });
        })
    </script>
