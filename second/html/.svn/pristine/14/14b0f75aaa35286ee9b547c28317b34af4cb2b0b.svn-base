/**/
.part2{
	overflow: hidden;
	background: #fff;
	border-bottom: 1px solid #e5e5e5;
}
.part3{
	overflow: hidden;
	background: #fff;
}
.part4{
	overflow: hidden;
	background: #fff;
	border-bottom: 0.25rem solid #e5e5e5;
}
/*垃圾桶*/
.header-bg-dustbin{
    background: url(../img/dustbin.png) 0rem 0rem no-repeat !important;
    background-size: 0.875rem auto !important;
}
.font-size-18{
	font-size: 0.9rem !important;
}
.color-black{
	color: #000 !important;
}
.pi-home{
	position: relative;
	height: 100%;
}
.pi-pic{
	position: absolute;
	top: 28%;
	left: 5%;
	height: 50%;
	width: 28.7%;
	-webkit-border-radius: 50%;
	background: url(../img/home-head-portrait.png) 0rem 0rem no-repeat;
	background-size: 100%;
	border: 1px solid #fff;
	z-index: 20;
}
.pi-pic img{
	height: 100%;
	width: 100%;
	-webkit-border-radius: 50%;
}
.pi-login{
	position: absolute;
	top: 84%;
	left: -2.25rem;
	height: 0.8rem;
	width: 4.5rem;
	-webkit-border-radius:100rem;
	background-color: rgba(0,0,0,0.35);
	border: 2px solid #fff;
	z-index: 20;
	font-size: 0.6rem;
	color: #fff;
}
.pi-login i{
	display: block;
	height: 1rem;
	width: 1rem;
	float: left;
	background: url(../img/home-icon.png) 0.4rem 0.2rem no-repeat;
	background-size: 0.6rem;
}
.personal-navigation{
	display: -webkit-box;
	padding: 0.5rem;
	line-height: 1rem;
	font-size: 0.7rem;
	white-space:nowrap; 
}
.pn-l,
.pn-r{
	height: 1rem;
	width: 0.9rem;
}
.pn-l.font1{
	background: url(../img/personal.png) 0rem 0.05rem no-repeat;
	background-size: 0.9rem;
}
.pn-l.font2{
	background: url(../img/personal.png) 0rem -1.9rem no-repeat;
	background-size: 0.9rem;
}
.pn-l.font3{
	background: url(../img/personal.png) 0rem -3.6rem no-repeat;
	background-size: 0.9rem;
}
.pn-l.font4{
	background: url(../img/personal.png) 0rem -5.4rem no-repeat;
	background-size: 0.9rem;
}
.pn-l.font5{
	background: url(../img/personal.png) 0rem -7.3rem no-repeat;
	background-size: 0.9rem;
}
.pn-l.font6{
	background: url(../img/personal.png) 0rem -8.9rem no-repeat;
	background-size: 0.9rem;
}
.pn-l.font7{
	background: url(../img/personal.png) 0rem -10.7rem no-repeat;
	background-size: 0.9rem;
}
.pn-l.font8{
	background: url(../img/personal3.png) 0rem 0.1rem no-repeat;
	background-size: 0.9rem;
}
.pn-l.font9{
	background: url(../img/personal3.png) 0rem -1.6rem no-repeat;
	background-size: 0.9rem;
}
.pn-r{
	background: url(../img/you-iocn.png) 0.35rem 0 no-repeat;
	background-size: 0.55rem;
}
.pn-btn{
	display: block;
	position: relative;
	height: 0.8rem;
	margin: 0.1rem 0rem;
	width: 1.8rem;
	-webkit-border-radius: 0.5rem;
	border: 1px solid #bcb5b5;
	overflow: hidden;
	background-color: #E5E5E5;
}
.pn-btn.true{
	background-color: #00baff;
}
.pn-btn i{
	display: block;
	position: absolute;
	top: 0rem;
	right: 0rem;
	height: 0.7rem;
	width: 0.7rem;
	-webkit-border-radius: 0.4rem;
	border: 1px solid #bcb5b5;
	background-color: #fff;
	-webkit-transition:-webkit-transform 0.3s;
}
.pn-btn.true i{
	-webkit-transform:translateX(-1rem);
	-webkit-transition:-webkit-transform 0.3s;
	
}
.pn-c1,
.pn-c2{
	-webkit-box-flex: 1;
}
.pn-c1{
	text-align: left;
	padding-left: 0.3rem;
	color: #000;
}
.pn-c2{
	text-align: right;
	padding-right: 0.3rem;
	color: #bcb5b5;
}
.pn-c1 b{
	margin: 0px 5px;
	color: #EB6877;
}
.pn-c1.btm02{
	margin-top: 0.2rem;
}
.pn-c2 .access{
	height: 1.4rem;
	line-height: 1.4rem;
	width: 3.6rem;
	float: right;
	background-color: #EB6877;
	color: #fff;
	text-align: center;
	-webkit-border-radius: 0.2rem;
}
.receivables-delivery{
	display: -webkit-box;
	padding: 0.5rem 0rem;
}
.pird{
	-webkit-box-flex: 1;
}
.pird a{
	display: block;
	position: relative;
    width: 2.5rem;
    margin: 0 auto;
    padding-top: 2.8rem;
    font-size: 0.6rem;
    color: #777777;
	white-space:nowrap; 
}
.pird a b{
	display: block;
	height: 0.8rem;
	line-height: 0.8rem;
	width: 0.8rem;
	position: absolute;
	right: 0.1rem;
	top: 0rem;
	color: #fff;
	background-color: #00baff;
	-webkit-border-radius: 100%;
}
.pird .nav1{
	background: url(../img/personal2.png) 0rem -0.02rem no-repeat;
    background-size: auto 2.5rem;
}
.pird .nav2{
	background: url(../img/personal2.png) -4.55rem 0.07rem no-repeat;
    background-size: auto 2.5rem;
}
.pird .nav3{
	background: url(../img/personal2.png) -8.95rem 0.07rem no-repeat;
    background-size: auto 2.5rem;
}
.pird .nav4{
	background: url(../img/personal2.png) -13.45rem 0.06rem no-repeat;
    background-size: auto 2.5rem;
} 
.pird .nav5{
	background: url(../img/personal2.png) -17.9rem -0.05rem no-repeat;
    background-size: auto 2.5rem;
}
.pi-mar{
	margin-bottom: 1.5rem;
}
.pi-pad{
	margin: 0rem 0.5rem;
}

.pf-text{
	width: 90%;
    height: 8rem;
    max-width: 90%;
    max-height: 8rem;
    min-width: 90%;
    min-height: 8rem;
    resize: none;
    padding: 0.2rem;
    color: #8f8f8f;
    font-size: 0.7rem;
	margin-top: 0.5rem;
	border: 1px solid #8f8f8f;
	-webkit-border-radius: 0.3rem;
}
.pf-text-lr{
	position: relative;
	display: -webkit-box;
	font-size: 0.7rem;
	padding: 0.5rem 1rem 0.5rem 0.5rem;
}
.pf-text-lr i{
	display: block;
	width: 3.5rem;
	margin-right: 0.5rem;
	line-height: 1rem;
	text-align: right;
}
.pf-text-lr .pf-text-lr-box{
	-webkit-box-flex: 1;
}
.pf-text-lr input{
	height: 1rem;
	line-height: 1rem;
	width: 100%;
    padding: 0rem 0.2rem;
	border: 1px solid #8f8f8f;
	-webkit-border-radius: 0.3rem;
}
.pf-text-lr textarea{
	width: 100%;
    height: 4rem;
    max-width: 100%;
    max-height: 4rem;
    min-width: 100%;
    min-height: 4rem;
    resize: none;
    padding: 0.2rem;
    color: #8f8f8f;
    font-size: 0.7rem;
	border: 1px solid #8f8f8f;
	-webkit-border-radius: 0.3rem;
}
.pf-pic{
	padding: 0rem 1rem;
	margin-top: 0.5rem;
}
.pf-pic img{
	display: block;
	float: left;
	height: 4rem;
	width: 4rem;
	margin-right: 0.5rem;
	border: 1px solid #e5e5e5;
}
.pf-pic a{
	position: relative;
	display: block;
	float: left;
	height: 2rem;
	line-height: 1rem;
	padding: 2rem 0.6rem 0rem 0.6rem;
	width: 2.8rem;
	background: url(../img/personal-add.png) 1.4rem 0.6rem no-repeat;
	background-size: 1.2rem;
	border: 1px dashed #e5e5e5;
	margin-right: 0.5rem;
	font-size: 0.6rem;
	color: #e5e5e5;
}
.pf-pic a input{
	position: absolute;
	top: 0;
	left: 0;
	height: 4rem;
	width: 4rem;
	opacity: 0;
}
.personal-access-content{
	padding: 0.5rem;
	font-size: 0.7rem;
	color: #777777;
	text-align: left;
}
.history-date{
	line-height: 2rem;
	font-size: 0.7rem;
	padding: 0rem 0.5rem;
	text-align: left;
	color: #c9c9c9;
}
.history-commodity{
	display: -webkit-box;
	padding: 0.5rem;
	border-top: 1px solid #bcb5b5;
}
.hc-pic{
	height: 5rem;
	width: 5rem;
	margin-right: 0.5rem;
}
.hc-btn{
	height: 0.8rem;
	width: 0.8rem;
	margin: 2.1rem 0.5rem 2.1rem 0rem;
	border: 1px solid #EB6877;
	-webkit-border-radius: 50%;
}
.hc-btn a{
	display: block;
	height: 0.6rem;
	width: 0.6rem;
	margin: 0.1rem;
	-webkit-border-radius: 50%;
}
.hc-btn.true a{
	background-color: #EB6877;
}
.hc-content{
	-webkit-box-flex: 1;
	font-size: 0.7rem;
	text-align: left;
}
.hc-content p{
	color: #777777;
}
.hc-content i{
	line-height: 2rem;
	color: #EB6877;
}

