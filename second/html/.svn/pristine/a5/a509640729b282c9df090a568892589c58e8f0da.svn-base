	border: 1px solid #00fffd;
	height: 1.2rem;
	line-height: 1.2rem;
	color: #00fffd;
	font-size: 0.6rem;
	-webkit-border-radius: 0.1rem;
}
.confirm-pop-ups.font1{
	width: 12rem;
	padding: 1.5rem 1rem;
}
.confirm-pop-ups.font2{
	width: 12rem;
	min-height: 2rem;
	padding: 5rem 1rem 0rem 1rem;
	background: rgba(0,0,0,0.65) url(../img/cpp-pic.png) 5.5rem 0.5rem no-repeat;
	background-size: 3rem;
}
.confirm-pop-ups.font3{
	padding: 0.5rem 1rem;
	width: 12rem;
}
.confirm-pop-ups.font4{
	width: 12rem;
	min-height: 3rem;
	padding: 4rem 1rem 0rem 1rem;
	background: rgba(0,0,0,0.65) url(../img/cpp-pic.png) 6rem 1rem no-repeat;
	background-size: 2rem;
}
.confirm-pop-ups.font5{
	width: 16rem;
	height: 10rem;
}
.confirm-pop-ups.font1 .box1{
	margin-top: 1.5rem;
}
.confirm-pop-ups.font5 .cpp-top{
	display: block;
	height: 1.5rem;
	line-height: 1.2rem;
	text-align: right;
	background-color: #000;
	-webkit-border-radius: 0.3rem 0.3rem 0 0;
}
.confirm-pop-ups.font5 .cpp-top i{
	color: #fff;
