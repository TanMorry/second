}
.confirm-pop-ups.font5 .finish{
	position: absolute;
	display: block;
	bottom: 0.75rem;
	left: 5rem;
	height: 1.5rem;
	line-height: 1.5rem;
	width: 6rem;
	background-color: #EB6877;
	color: #fff;
	font-size: 0.9rem;
	-webkit-border-radius: 0.3rem;
}


