/*筛选和商品列表的价格下拉列表*/
.nav-btn{display:-webkit-box;-webkit-box-pack:center;-webkit-box-align: center;background: #FFFFFF;position: relative }
.nav-btn>a{font-size: 0.7rem;display: block;color:#9b9b9b;padding:0.7rem 0;-webkit-box-flex:1;border-bottom:0.25rem solid #e7e7e7;position: relative;width: 33.33%}
.nav-btn .nav-btn-color{border-bottom: 0.25rem solid #ea6673;color: #ea6673 }
.nav-btn .nav-btn-a:before{height:0;width: 0;border: 0.3rem solid transparent;position: absolute;
	content: "";display: block;border-top-color:#979595}
.nav-btn .nav-btn-b:before{border-top-color:#ea6673 }
.nav-btn .position-a:before{left:91%;top:49.5%;}
.nav-btn .position-c:before{left: 70%;top: 49.5%;}
.nav-btn-ul{position: absolute;left: 0;top:98%;background: #FFFFFF;width: 100%;text-align: left;display: none}
.nav-btn-ul li{;border-bottom: 1px solid #949293;padding-left: 0.7rem}
.nav-btn-ul a{font-size: 0.7rem;color:#949293;;width: 100%;padding:0.7rem 0;display: block }


/*遮罩层*/
.content-cover{position:fixed;left: 0;top: 0;width: 100%;z-index:3;height:100%;background: rgba(20, 20, 20, 0.82);display: none;margin-top:2rem;}
.content-cover2{position:fixed;left: 0;top: 0;width: 100%;z-index:3;height:100%;background: rgba(255, 255, 255, 0.02);display: none;margin-top:2rem;}
/*加载失败*/
.load-fail{
	font-size: 0.7rem;
	color:#979595;
	padding: 10% 0;
}

/*tl loading图片样式*/
#loading{
	background-color:#fff;
	height:100%;
}
#loading img{
	width:auto !important;
}

.error{
	display:-webkit-box;
	-webkit-box-pack: center;
	-webkit-box-align: center;
	text-align: center;
	padding:2rem 0;
}
.error>i{
	display: block;
    width: 10%;
	height: 10%;
	background: url("../img/error.png") 0 0 no-repeat;
	background-size: 100%;
	vertical-align: middle;
}
.error a{
    display: block;
	font-size: 0.8rem;
	color: grey;
	padding-left:0.5rem ;
}

/*取消订单的弹窗*/
.cancel-pos{position: relative}
.cancel-reason{position:absolute;bottom:0;left: 0;z-index: 13;background: #e5e5e5;width: 100%;display:none;border-top: 0.2rem solid #e5e5e5}
.cancel-reason-top{padding: 0.3rem 0.5rem;background: #ffffff;text-align: right}
.cancel-reason-top>a {font-size: 0.9rem;color:#00baff;}
.cancel-block{display: block}
.cancel-reason-bottom >a{font-size: 0.8rem;color:#777777;;display: block;padding: 0.5rem 0 }


