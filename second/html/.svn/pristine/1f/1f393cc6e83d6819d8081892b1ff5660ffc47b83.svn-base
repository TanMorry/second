<script>
var userid;
var username;
	$(function(){
		var mySwiper = new Swiper('.swiper-container', {
			autoplay: 2000,
			speed: 500,
			autoplayDisableOnInteraction: false,
			autoHeight: true,
			loop: true,
			pagination : '.swiper-pagination',
		});

		var mySwiper = new Swiper('.top',{
			pagination: '.top-one',
			paginationClickable: true,
			paginationBulletRender: function (index, className) {
				switch (index) {
					case 0: name='欧美榜';break;
					case 1: name='国韵榜';break;
					case 2: name='日韩亚榜';break;
					case 3: name='原创榜';break;
					default: name='';
				}
				return '<div class=' + className + '>' + '<a href="#">'+name+'</a>' + "</div>";
			},
			onSlideChangeEnd:function(swiper)
			{
				$(swiper.wrapper[0].children[swiper.activeIndex]).find('a.up-top').attr
				('href','Battlefield/top-usa.html?type='+swiper.activeIndex);
				console.log(swiper.activeIndex);
			}
		})

		$('.header-in input').on('click',function(){
			window.location.href='sousuo-index.html'//filterType=1
		})

		IndexList();
//      $("#Info").html(info);
	})
//	function getUserInfo(id,name)
