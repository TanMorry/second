	function IndexList(id,name)
	{
		userid = id;
		username = name;
		var url = "/tongrenmiao/index.php?g=Mall&m=index&a=main";
		$.ajax({
			url:url,
			data:{},
			dataType:'json',
			type:'post',
			beforeSend:function()
			{
				$("#wrapper").hide();
				$("#searchHead").hide();
				$("#loading").show();
//				$('body').addClass('loading');
			},
			success:function(ret)
			{
				/*-----排行榜  start-----*/
				$("#usa").html(handleRank(ret.rank_list[0]));
				$("#country").html(handleRank(ret.rank_list[1]));
				$("#jp").html(handleRank(ret.rank_list[2]));
				$("#our").html(handleRank(ret.rank_list[3]));
				/*-----排行榜  end-----*/

				$("#newProduct").html(handleHtml(ret.new));
				$("#moods").html(handleHtml(ret.moods));
				$("#zhoubian").html(handleHtml(ret.PopularityRound));
				$("#sales").html(handleHtml(ret.moodsAndAround));
				$("#wrapper").show();
				$("#searchHead").show();
				$("#loading").hide();
//					$('body').removeClass('loading');
				wrapper.refresh();
			},
//			error:function()
//			{
//				location.href = 'error.html?status=2'
//			}
		})
	}

	//排行榜处理
	function handleRank(list)
	{
		var str = "<ul>";

		var j = 0;
		for(var i in list)
		{
			if(j == 3)
			{
				break;
			}
			str += ' <li class="top-two-li horizontal-center top-bottom">\
						<a href="#" class="up-one up-one-'+j+'" ></a>\
						<a href="#" class="'+moreClassName(list[i].mchange)+'"></a>\
						<span class="up-sp1"><a href="Battlefield/top-yc.html">'+list[i].cpname+'</a></span>\
							<i class="up-auto up-auto-a"></i>\
						<span class="up-sp2">'+list[i].mcut+'</span>\
					</li>'
			j++;

		}
		str += '</ul>';

		var str2 = '<ul class="dashed-top">';
		j=0;
		for(var i in list)
		{
			if(j>= 3)
			{
				str2 += '<li class="top-two-li horizontal-center top-bottom">\
						<a href="#" class="up-one up-one-'+j+'"></a>\
						<a href="#" class="'+moreClassName(list[i].mchange)+'"></a>\
						<span class="up-sp1">'+list[i].cpname+'</span>\
						<span class="up-sp2">'+list[i].mcut+'</span>\
					</li>';
			}
			j++;
		}
		console.log(j)
		str2 += '<li><a href="Battlefield/top-usa.html?type=0" class="up-top" >查看更多排名>></a></li><ul>';

		return str+str2;
	}

	//添加类型显示相应的图片
	function moreClassName(state)
	{
		var str ;
		if(state == 0)
		{
			str = 'up-three up-three-a';
		}
		else if(state == 1)
		{
			str = 'up-two up-two-a';
		}
		else if(state == -1)
		{
			str = 'up-two up-two-b';
		}
		return str;
	}

	//处理html
	function handleHtml(list)
	{
		var str = "";
		$.each(list,function(key,val){
			str +='<li>\
						<div class="sale-ones-front">\
							<p class="sale-of-img"><a href="detail-page1.html?id='+val.id+'"><img src="'+val.pic+'"></a></p>\
							<p class="sale-of-name">'+val.name+'</p>\
							<p class="sale-of-text">原著:伪装者</p>\
							<p class="sale-of-text">CP圈:楼城</p>\
							<p class="sale-of-price">'+val.price+'</p>\
						</div>\
					</li>'
		})
		return str;
	}
</script>
