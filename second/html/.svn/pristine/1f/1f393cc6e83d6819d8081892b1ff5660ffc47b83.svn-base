			var opacityHeader ;
			opacityMask = positionSwiper / changeY ;
			$('.header-bg').css('background','rgba(235,104,119,' + opacityMask + ')');
		}
		else {
			$('.header-bg').css('background','rgba(235,104,119,1)');
		}

	}

