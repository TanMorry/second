

.containe{
	display: -webkit-box;
	position: absolute;
	width: 100%;
	height: 100%;
}
.sidebar-l{
	position: relative;
	width: 3.5rem;
}
.sidebar-list{
	overflow-y: auto;
	position: absolute;
	width: 100%;
	height: 100%;
}
.sidebar-list li a{
	display: block;
	/*width: 3.5rem;*/
	height: 2rem;
	line-height: 2rem;
	border-right: 1px solid #a0a0a0;
	font-size: 0.7rem;
	color: #6c6868;
}
.sidebar-list li a.fixed{
	color: #fff;
	background: #eb6877;
}
.sidebar-r{
	position: relative;
	-webkit-box-flex: 1;
}
.sidebar-ab{
	overflow-y: auto;
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}
.sidebar-r .sidebar-one:first-child{
	margin-top: 0.5rem;
}

.sidebar-o-cap{
	text-align: left;
	height: 1rem;
	line-height: 1rem;
	margin: 0 0 0.5rem 0.375rem;
	padding-left: 0.3rem;
	border-left: #eb6877 solid 0.2rem;
	font-size: 0.8rem;
	color: #6c6868;
}
.sidebar-o-item li{
	float: left;
	-webkit-box-sizing:border-box;
	box-sizing: border-box;
	width: 33.33%;
	margin-bottom: 1rem;
	padding: 0 0.375rem;
}
.sidebar-o-item li a{
	position: relative;
	display: block;
	width: 100%;
	max-width: 8.05rem;
}
.sidebar-o-item li a i{
	position: absolute;
	left: 0;
	bottom: 0;
	width: 100%;
	line-height: 1rem;
	font-size: 0.7rem;
	color: #fff;
	background: rgba(0,0,0,0.7);
}



