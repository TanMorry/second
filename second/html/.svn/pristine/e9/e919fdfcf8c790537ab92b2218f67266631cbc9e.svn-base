@charset "utf-8";
/*通用段落格式*/
.part{
	overflow: hidden;
	margin-bottom: 0.25rem;
	background: #fff;
}
.part.middle-bottom{
	margin-bottom: 0.75rem;
}
.part.high-bottom{
	margin-bottom: 1.25rem;
}
/*网页头*/
.header-bg{
	display:-webkit-box;
	display:-moz-box;
}
.header-c{

	-webkit-box-flex: 1;
	display:-webkit-box;
	-webkit-box-orient:horizontal;
	-webkit-box-pack:center; 
	-webkit-box-align:center;
}
.header-c>*{
	display: block;
}
.header-c.marR{
	margin-right: 1.6rem;
}
/*主页头部样式*/
.header-bg.home{
	width: 100%;
	position: absolute;
	z-index: 10;
	/*background: rgba(235,104,119,1);*/
}
.header-bg.home .header-l,
.header-bg.home .header-r{
	padding: 0.3rem 0.25rem;
}
.header-bg.home .header-l a,
.header-bg.home .header-r a{
	display: block;
	width: 1.375rem;
	height: 1.375rem;
	/*margin: 0.3rem 0.25rem;*/
}
.header-bg.home .header-l a{
	background: url(../img/header.png) 0 0 no-repeat;
	background-size: 1.4rem;
	margin-left: 0.5rem;
}
.header-bg.home .header-r a{
	background: url(../img/header.png) 0 -1.4rem no-repeat;
	background-size: 1.4rem;
	margin-right: 0.5rem;
}
.header-bg.home .header-c input{
	display: block;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	width: 90%;
	height: 1.4rem;
	padding-left: 1.4rem;
	-webkit-border-radius: 0.25rem;
	border-radius: 0.25rem;
	background: rgba(255,255,255,0.7) url(../img/search.png) 0 0 no-repeat;
	background-size: 1.4rem;
}

/*普通头部样式*/
.header-bg.normal{
	background: #eb6877;
}
.header-bg.normal .header-l,
.header-bg.normal .header-r{
	padding: 0.4rem 0.3rem;
}
.header-bg.normal .header-l a{
	display: block;
	width: 1rem;
	height: 1rem;
}
.header-bg.normal .header-r>a{
	display: block;
	width: 1rem;
	height: 1rem;
}
.header-bg.normal .header-l a{
	background: url(../img/header4.png) 0 0 no-repeat;
	background-size: 1rem auto;
}
.header-bg.normal .header-r>a{
	background: url(../img/header4.png) 0rem -1.7rem no-repeat;
	background-size: 1rem auto;
}
.header-bg .header-c input{
	display: block;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	width: 100%;
	height: 1.075rem;
	line-height: 1.075rem;
	padding-left: 0.5rem;
	-webkit-border-radius: 0.25rem;
	border-radius: 0.25rem;
	background: #fff;
	font-size: 0.7rem;
}
/*带搜索背景的样式*/
.header .normal .header-c input.search{
	padding-left: 2rem;
	background: #fff url(../img/search.png) 0.2rem center no-repeat;
	background-size: 0.875rem;
}
.header .normal .header-r p{
	font-size: 0.6rem;
	color: #fff;
	margin: 0.08rem 0rem;
	cursor: pointer;
}
/*普通样式的叉子键*/
.header-X{
	position: absolute;
	display: block;
	height: 0.8rem;
	line-height: 0.8rem;
	width: 0.8rem;
	top: 0.5rem;
	right: 0.3rem;
	padding: 0rem;
	background-color: #fff;
	-webkit-border-radius: 0.55rem;
}
.header-X a{
	display: block;
	background-color: #bcb5b5;
	-webkit-border-radius: 0.5rem;
	color: #fff;
	text-align: center;
	font-size: 0.7rem;
}
/*内部头部样式*/
.header-bg.internal{
	background: #fff;
	margin-bottom: 0.125rem;
}
.header-bg.internal .header-l,
.header-bg.internal .header-r{
	padding: 0.3375rem 0.125rem;
}
.header-bg.internal .header-l a,
.header-bg.internal .header-r a{
	display: block;
	width: 0.875rem;
	height: 0.875rem;
	margin: 0rem 0.2rem;
	padding: 0.2rem;
}
.header-bg.internal .header-l a{
	background: url(../img/header3.png) 0.15rem 0.15rem no-repeat;
	background-size: 1rem;
}
.header-bg.internal .header-r a{
	background: url(../img/header3.png) 0.15rem -1.55rem no-repeat;
	background-size: 1rem;
}
.header-bg.internal .header-c p{
	display: block;
	font-size: 0.8rem;
	color: #eb6877;
	overflow: hidden;
	white-space:nowrap; 
}
/*带购物车按钮的样式*/
.header-bg.internal .header-c p.cart{
	padding-left: 1.8rem;
}
.header-bg.internal .header-c p.detail{
	padding-right: 1.8rem;
}
.header-bg.internal .header-r .cart{
	background: url(../img/header3.png) 0.15rem -3.25rem no-repeat;
	background-size: 1rem;
}

.header-bg.internal .header-r .disabled{
	background: #fff;
	background-size: 0.875rem;
}
/*确认订单样式*/
.header-bg.confirm{
	background: #EB6877;
	margin-bottom: 0.125rem;
}
.header-bg.confirm .header-l{
	padding: 0.3375rem 0.125rem;
}
.header-bg.confirm .header-l a{
	display: block;
	width: 0.875rem;
	height: 0.875rem;
}
.header-bg.confirm .header-l a{
	background: url(../img/header4.png) 0 0.1rem no-repeat;
	background-size: 0.875rem;
}
.header-bg.confirm .header-c p{
	display: block;
	margin-right: 1.125rem;
	font-size: 0.8rem;
	color: #fff;
	overflow: hidden;
	white-space:nowrap; 
}
/*悬浮样式*/
.header-bg.float{
	position: absolute;
	width: 100%;
	z-index: 10;
}
.header-bg.float .header-l,
.header-bg.float .header-r{
	padding: 0.3375rem 0.125rem;
}
.header-bg.float .header-l a,
.header-bg.float .header-r a{
	display: block;
	width: 0.875rem;
	height: 0.875rem;
	margin: 0rem 0.2rem;
	padding: 0.2rem;
	-webkit-border-radius: 0.6rem;
	filter: alpha(opacity:65);
    opacity: 0.65;
}
.header-bg.float .header-l a{
	display: block;
	background: #000000 url(../img/header4.png) 0.15rem 0.15rem no-repeat;
	background-size: 1rem;
}
.header-bg.float .header-r a{
	background: #000000 url(../img/header4.png) 0.15rem -1.55rem no-repeat;
	background-size: 1rem;
}
.header-bg.float .header-r .cart{
	background: #000000 url(../img/header4.png) 0.15rem -3.25rem no-repeat;
	background-size: 1rem;
}
/*悬浮样式2*/
.header-bg.float2{
	position: absolute;
	width: 100%;
	z-index: 10;
}
.header-bg.float2 .header-l,
.header-bg.float2 .header-r{
	padding: 0.3375rem 0.125rem;
}
.header-bg.float2 .header-l a,
.header-bg.float2 .header-r a{
	display: block;
	width: 0.875rem;
	height: 0.875rem;
	margin: 0rem 0.2rem;
	padding: 0.2rem;
}
.header-bg.float2 .header-l a{
	background: url(../img/header3.png) 0.15rem 0.15rem no-repeat;
	background-size: 1rem;
}
.header-bg.float2 .header-r a{
	background: url(../img/header3.png) 0.15rem -1.55rem no-repeat;
	background-size: 1rem;
}
.header-bg.float2 .header-r .cart{
	background: url(../img/header3.png) 0.15rem -3.25rem no-repeat;
	background-size: 1rem;
}
/*悬浮样式3*/
.header-bg.float3{
	position: absolute;
	width: 100%;
	z-index: 10;
}
.header-bg.float3 .header-l,
.header-bg.float3 .header-r{
	padding: 0.3375rem 0.125rem;
}
.header-bg.float3 .header-l a,
.header-bg.float3 .header-r a{
	display: block;
	width: 1.4rem;
	height: 1.4rem;
	margin: 0rem 0.2rem;
}
.header-bg.float3 .header-l a{
	background: url(../img/header6.png) 0rem 0rem no-repeat;
	background-size: 1.4rem;
}
.header-bg.float3 .header-r a{
	background: url(../img/header6.png) 0rem -2.65rem no-repeat;
	background-size: 1.4rem;
}
.header-bg.float3 .header-c p{
	display: block;
	line-height: 1.4rem;
	font-size: 0.8rem;
	color: #fff;
	overflow: hidden;
	white-space:nowrap; 
}
/*悬浮样式遮罩*/
.header-mask{
	height: 2rem;
	width: 100%;
	position: absolute;
	background-color: #fff;
	z-index: 9;
	filter: alpha(opacity:0);
    opacity: 0;
    border-bottom: 1px solid #e5e5e5;
}
/*悬浮样式遮罩2*/
.header-mask2{
	height: 2rem;
	width: 100%;
	position: absolute;
	background-color: #EB6877;
	z-index: 9;
	filter: alpha(opacity:0);
    opacity: 0;
}
/*悬浮样式弹窗*/
.header-pop-ups{
	display: none;
	position: absolute;
	width: 5rem;
	filter: alpha(opacity:65);
    opacity: 0.65;
    right: 0.5rem;
    top: 1.7rem;
}
.hpp-arrow{
	margin-left: 3.9rem;
	border-bottom:0.5rem solid #000000 ;
	border-left:0.5rem solid transparent ;
	border-right:0.5rem solid transparent ;
	border-top:0px solid transparent ;
}
.hpp-content{
	height: 1rem;
	padding: 0.5rem;
	background-color: #000000;
	font-size: 0.7rem;
	color: #fff;
	text-align: left;
}
.hpp-content.border-dashed{
	border-bottom: 1px dashed #fff;
}
.hpp-content i{
	float: left;
	height: 1rem;
	width: 1rem;
	margin-right: 0.5rem;
}
.hpp-content .nav1{
	background: url(../img/header5.png) 0rem 0.2rem no-repeat ;
	background-size: 1rem;
}
.hpp-content .nav2{
	background: url(../img/header5.png) 0rem -1.9rem no-repeat ;
	background-size: 1rem;
}
.hpp-content .nav3{
	background: url(../img/header5.png) 0rem -4.0rem no-repeat ;
	background-size: 1rem;
}

/*正常页脚样式*/
.foot{
	display: -webkit-box;
	padding: 0.2rem 0;
	border-top: 1px solid #e5e5e5;
}
.foot div{-webkit-box-flex: 1;}
.foot div a{
	display: block;
	line-height: 0.35rem;
	/*1.625*/
	padding-top: 0.9rem;
	font-size: 0.7rem;
	color: #8c8b8b;
}

.foot .nav1{
	background: url(../img/footer.png) center 0.1rem no-repeat;
	background-size: 0.875rem auto;
}
.foot .nav2{
	background: url(../img/footer.png) center -3.75rem no-repeat;
	background-size: 0.875rem auto;
}
.foot .nav3{
	background: url(../img/footer.png) center -7.4rem no-repeat;
	background-size: 0.875rem auto;
}
.foot .nav4{
	background: url(../img/footer.png) center -11rem no-repeat;
	background-size: 0.875rem auto;
}
.foot .fixed .nav1{
	color: #eb6877;
	background: url(../img/footer.png) center -0.1rem no-repeat;
	background-size: 0.875rem auto;
}
.foot .fixed .nav2{
	color: #eb6877;
	background: url(../img/footer.png) center -3.85rem no-repeat;
	background-size: 0.875rem auto;
}
.foot .fixed .nav3{
	color: #eb6877;
	background: url(../img/footer.png) center -7.6rem no-repeat;
	background-size: 0.875rem auto;
}
.foot .fixed .nav4{
	color: #eb6877;
	background: url(../img/footer.png) center -11.35rem no-repeat;
	background-size: 0.875rem auto;
}

/*购物车页脚*/
.foot-border{
	border-top: 1px solid #e5e5e5;
}
.foot-border2{
	border-top: 0.25rem solid #e5e5e5;
}
.foot-left{
	display: -webkit-box;
	float: left;
	width: 6rem;
	padding: 0.2rem 0;
}
.foot-left div{-webkit-box-flex: 1;}
.foot-left div a{
	display: block;
	line-height: 0.7rem;
	/*1.625*/
	padding-top: 1.8rem;
	font-size: 0.7rem;
	color: #8c8b8b;
}
.foot-left .nav1{
	background: url(../img/footer2.png) center 0 no-repeat;
	background-size: 1.75rem auto;
	border-right: 1px solid #777777;
}
.foot-left .nav2{
	background: url(../img/footer2.png) center -3.85rem no-repeat;
	background-size: 1.75rem auto;
}
.foot-right{
	display: -webkit-box;
	float: right;
	width: 10rem;
	padding: 0.2rem 0;
}

.foot-right div{-webkit-box-flex: 1;padding: 0.25rem 0rem;}
.foot-right div a{
	display: block;
	line-height: 2rem;
	/*1.625*/
	height: 2.0rem;
	font-size: 0.7rem;
	color: #fff;
}
.foot-right .nav1{
	background-color: #facd89;
	margin: 0rem 0.25rem;
	-webkit-border-radius: 0.25rem;
	width: 4.5rem;
}
.foot-right .nav2{
	background-color: #eb6877;
	margin: 0rem 0.25rem;
	-webkit-border-radius: 0.25rem;
	width: 4.5rem;
}
/*支付页脚*/
.foot-pay{
	display: -webkit-box;
	height: 2.5rem;
	line-height: 2.5rem;
	font-size: 0.7rem;
	padding: 0rem 0.5rem;
}

.foot-pay p{
	display: block;
	text-align: left;
	-webkit-box-flex: 1;
	white-space:normal ; 
	color: #777777;
}

.foot-pay b{
	white-space:normal ; 
	color: #EB6877;
}

.foot-pay a{
	-webkit-box-flex: 1;
	display: block;
	height: 2rem;
	line-height: 2rem;
	min-width: 5rem;
	margin: 0.25rem 0.25rem;
	-webkit-border-radius: 0.25rem;
	background-color: #EB6877;
	white-space:nowrap ; 
	color: #fff;
	font-size: 0.8rem;
}
/*选择页面*/
.footer-chooseAll{
	display: -webkit-box;
}
.footer-chooseAll a{
	display: block;
	-webkit-box-flex: 1;
	line-height: 2.5rem;
	font-size: 0.9rem;
	color: #fff;
}
.footer-chooseAll a.font1{
	background-color: #00baff;
}
.footer-chooseAll a.font2{
	background-color: #eb6877;
}
/*翻页页脚*/
.page{
	display:-webkit-box;
	-webkit-box-orient:horizontal; 
	-webkit-box-pack:center; 
	-webkit-box-align:center;
	padding: 0.25rem 0;
	border-top: 1px solid #e5e5e5;
}
.header-fot {
	color: #ffffff;
	font-size: 0.8rem;
}

.page>*{
	display: block;
}
.page-up{
	width: 1.6rem;
	height: 1.6rem;
	margin-left: 5%;
	background: url(../img/page.png) 0 0 no-repeat;
	background-size: auto 1.6rem;
}
.page-down{
	width: 1.65rem;
	height: 1.6rem;
	margin-right: 5%;
	background: url(../img/page.png) -2rem 0 no-repeat;
	background-size: auto 1.6rem;
}
.page-write{
	-webkit-box-flex: 1;
	min-width: 1.6rem;
	max-width: 5.5rem;
	height: 1.6rem;
	margin-right: 0.25rem;
	-webkit-border-radius: 0.25rem;
	border-radius: 0.25rem;
	background: #c9c9c9;
}
.updisable .page-up{
	width: 0rem;
}
.updisable .page-write{
	margin-left: 1.6rem;
}
.downdisable .page-down{
	width: 0rem;
}
.downdisable .page-count{
	margin-right: 1.65rem;
}

.page-go{
	height: 1.6rem;
	line-height: 1.6rem;
	padding: 0 0.5rem;
	margin-right: 0.25rem;
	-webkit-border-radius: 0.25rem;
	border-radius: 0.25rem;
	font-size: 0.8rem;
	color: #fff;
	background: #eb6877;
}
.page-count{
	height: 1.6rem;
	line-height: 1.6rem;
	font-size: 0.8rem;
	color: #c9c9c9;
}
.auto-width{
	-webkit-box-flex: 1;
	overflow: hidden;
	min-width: 0.25rem;
	height: 0;
}

.navigation{
	padding: 0.6rem 0;
}
.nav-single{
	float: left;
	width: 33.33%;
}
.nav-single a{
	display: block;
	width: 3.8rem;
	margin: 0 auto;
	padding-top: 3.8rem;
	font-size: 0.75rem;
	color: #777777;
}
.nav-single a i{
	display: block;
}
.navigation .nav1{
	background: url(../img/navigation.png) 0.13rem 0 no-repeat;
	background-size: auto 3.5rem;
}
.navigation .nav2{
	background: url(../img/navigation.png) -4.53rem 0 no-repeat;
	background-size: auto 3.5rem;
}
.navigation .nav3{
	background: url(../img/navigation.png) -9.05rem 0 no-repeat;
	background-size: auto 3.5rem;
}

.sale-item{
	padding: 0 0.375rem;
}
.sale-item2{
	padding: 1.5rem 0.375rem 0;
}
.sale-cap{
	padding: 0.5rem 0.25rem;
}
.sale-c-l{
	float: left;
	width: 7.05rem;
	height: 1.4rem;
}
.sale-c-l.cap-news{
	background: url(../img/xinp-tuijian.png) 0 0.2rem no-repeat;
	background-size: 65%;
}
.sale-c-l.cap-hots{
	background: url(../img/renqi-tongren.png) 0 0.2rem no-repeat;
	background-size:70%;
}
.sale-c-l.cap-renqi{
	background: url(../img/renqi-zb.png) 0 0 no-repeat;
	background-size:65%;
}
.sale-c-r{
	float: right;
	height: 1.4rem;
	line-height: 1.4rem;
	font-size: 1rem;
}
.sale-c-r.cap-red{
	color: #eb6877;
	font-size: 0.8rem;
}
.sale-c-r.cap-yellow{
	color: #f8b551;
	font-size: 0.8rem;
}
.sale-c-r img{
	display: block;;
	height:0.75rem;
	width: 2.3rem;
	margin-top: 0.3rem;
}

.sale-ones>li{
	float: left;
	-webkit-box-sizing:border-box;
	box-sizing: border-box;
	width: 33.33%;
	margin-bottom: 1rem;
	padding: 0 0.25rem;
}
.sale-ones.tie>li{
	width: 50% !important;
	max-width: 12.55rem;
}
.sale-ones-front{
	display: block;
	max-width: 8.05rem;
	margin: 0 auto;
}
.sale-ones-front.tie{
	max-width: 12.55rem;
	margin: 0 auto;
}
.sale-ones.tie>li:nth-child(odd){
	float: left;
}
.sale-ones.tie>li:nth-child(even){
	float: right;
}
.sale-of-img{
	position: relative;
	overflow: hidden;
	margin-bottom: 0.5rem;
	border: 1px solid #777777;
	background: #eeeeee;
}
.sale-of-img img{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}
.sale-of-img{padding-bottom: 100%;}

.sale-of-price,
.sale-of-text,
.sale-of-name{
	text-align: left;
	display: -webkit-box;
	-webkit-line-clamp: 1;
	-webkit-box-orient: vertical;
	/*text-overflow: ellipsis;*/
	overflow: hidden;
}
.sale-of-name{
	height: 0.75rem;
	line-height: 0.75rem;
	margin-bottom: 0.2rem;
	font-size: 0.75rem;
	color: #777777;
}
.sale-of-text{
	height: 0.7rem;
	line-height: 0.7rem;
	margin-bottom: 0.1rem;
	font-size: 0.65rem;
	color: #c9c9c9;
}
.sale-of-price{
	height: 0.65rem;
	line-height: 0.65rem;
	margin-bottom: 0;
	font-size: 0.65rem;
	color: #eb6877;
	margin-top: 0.3rem;
}

.shop-more{
	padding: 0.5rem 0 1.5rem;
}
.shop-m-single{
	float: left;
	-webkit-box-sizing:border-box;
	box-sizing: border-box;
	width: 50%;
	padding: 0 0.375rem;
}
.shop-m-single a{
	display: block;
	overflow: hidden;
	max-width: 25rem;
	margin: 0 auto;
}
.shop-m-single a i{
	display: block;
	margin-top: 40%;
}

.shop-ms-store{
	background: url(../img/shop-more.png) 0 0 no-repeat;
	background-size: 100%;
}
.shop-ms-custom{
	background: url(../img/shop-more.png) 0 100% no-repeat;
	background-size: 100%;
}

.shopping{
	display: -webkit-box;
	padding: 0.8rem 0.375rem 1rem;
}
.sp-l,
.sp-r{
	-webkit-box-flex: 1;
	/*margin 1px补height*/
	margin-top: 1px;
	height: 1px;
	background: #ff7699;
}
.sp-c{
	width: 10.1rem;
	/*android4.4以下小屏幕可能不会缩放*/
	max-width: calc(100% - 1rem);
	height: 2.05rem;
	margin: 0 0.5rem;
	background: url(../img/zldxbbd.png) 0 0 no-repeat;
	background-size: contain;
}
.sp-w{
	font-size: 0.7rem;
	color: #ff7699;
	word-spacing: 0.5rem;
	margin: 0rem 0.1rem;
}

.think-to-first{
	display: -webkit-box;
	border-bottom: 1px solid #bcb5b5;
}
