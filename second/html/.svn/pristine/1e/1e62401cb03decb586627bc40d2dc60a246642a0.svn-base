<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">

<meta name="viewport" content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" />
<meta name="format-detection" content="telephone=no"/>
<title>喵</title>
<link href="css/global.css" rel="stylesheet" type="text/css">
<link href="css/public.css" rel="stylesheet" type="text/css">
<link href="css/hot-renqiben.css" rel="stylesheet" type="text/css">
<link href="css/shop-fenlei.css" rel="stylesheet" type="text/css">
<link href="css/personal.css" rel="stylesheet" type="text/css">
<style type="text/css">
</style>

<script src="js/jquery-2.2.4.min.js"></script>
<script>
    $().ready(function(){
        $('.header-r').on('click', '.header-bg-btn', function(event) {
            if( $(".header-r-tc").css('display') == 'none' ){
                $(".header-r-tc").css('display','block')
            }
            else{
                $(".header-r-tc").css('display','none');
            }
        });
        $('.content').on('click', function(event) {
            $(".header-r-tc").css('display','none');
        });
        $('.footer').on('click', function(event) {
            $(".header-r-tc").css('display','none');
        });
        
        $('.personal-navigation').on('click', '.pn-btn', function(event){
        	$(this).toggleClass('true');
        });
    })
</script>
</head>
<body>
<!-- 首页 -->
<section class="flex">
    <section class="header">
        <div class="header-bg normal">
            <div class="header-l"><a href="search/shangyedian.html"></a></div>
            <div class="header-c header-fot">设置</div>
            <div class="header-r">
                <a  href="#" class="header-bg-btn"></a>
                <div class="header-r-tc">
                    <a href="#" class="header-tc-a"><img src="img/xinfeng.png">消息</a>
                    <a href="main.html" class="header-tc-b"><img src="img/shouy.png">首页</a>
                </div>
            </div>
        </div>
    </section>
    <!-- 主体 -->
    <section class="content flex">
        <a class="personal-navigation part2" href="#">
			<div class="pn-c1">清除本地缓存</div>
			<div class="pn-c2 color-black">2.16M</div>
		</a>
        <div class="personal-navigation part2">
			<div class="pn-c1">版本信息</div>
			<div class="pn-c2 color-black">V1.2</div>
		</div>
        <div class="personal-navigation part2">
			<div class="pn-c1">接受新消息通知</div>
			<a class="pn-btn true" href="#"><i></i></a>
		</div>
        
    </section>
    <section class="footer">
        <div class="footer-bottom">
            <a class="font-size-18" href="#" >退出登录</a>
        </div>
    </section>
</section>
</body>
</html>