.header-bg .header-c input{
    padding-left: 2rem;
    background: #fff url(../img/search.png) 0.2rem center no-repeat;
    background-size: 1.75rem;
}
.content{background: #ffffff; box-sizing: border-box; }
.nav-more{position: relative;width: 100%;}
.nav-more>img{width: 100%;display:block}
.nav-more .list{position:absolute; bottom: 0;left: 0;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    width: 100%;
    background: rgba(66, 66, 66, 0.7);color: #fafafa;padding: 0 0.7rem;
}

.list-l,.list-r{
    display:-webkit-box;
    -webkit-box-align: center;
    -webkit-box-orient:horizontal;
    font-weight: bold;
}
.list-l{float: left;height: 2.5rem;line-height: 2.5rem;}
.list-l span{font-size: 0.7rem;display:block }
.list-l a{display:block;font-size: 0.7rem;width: 3.2rem;height: 1.5rem;;
    line-height: 1.5rem;margin: 0 0.4rem;border-radius:0.3rem;
    color: #ffffff;}
.list-bg1 a{background:#79defc }
.list-bg2 a{background:#ff7698 }


.list-r{float: right}
.list-follow p{
    line-height: 1.25rem;
    font-size: 0.7rem;
}
.list-btn{
    display: block;
    width: 4.1rem;
    height: 1.85rem;
    background: url(../img/wujiaoxing.png) 0 0 no-repeat;
    background-size: 100%;
}




.main-infor>li{font-size: 0.9rem;color: #777777;text-align: left;margin: 1.2rem 0;}
.main-infor{ padding:0  0.7rem;word-break:break-all;margin-top:2rem }

.main-img-d div{margin: 1rem auto;}






.footer-bottom a{color: #FFFFFF;font-size: 1.2rem;display: block;height: 2.5rem;background:#eb6877;line-height: 2.5rem;text-align: center;}
